                                                <input type="text" class="form-control" id="Invoice_address" name="Invoice_address" value="@Model.SalesReturnMasterObj.Invoice_address" placeholder="Customer Address">
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">CustomerTRN</label>
                                            <div class="col-sm-8">
