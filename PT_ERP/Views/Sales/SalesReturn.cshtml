                                            </div>
                                        </div>
                                        @*<div class="row MasterForm">
                                                <label for="shipping_co_ID" class="col-sm-4 col-form-label">Shipping Co</label>
                                                <div class="col-sm-8">
                                                    <select class="form-control" id="shipping_co_ID" name="shipping_co_ID" asp-items="@(new SelectList( ViewBag.ShippingCompanies,"Partner_ID","PartnerName", Model.SalesReturnMasterObj.shipping_co_ID))">
                                                        <option>Select Ship Co</option>

                                                    </select>

                                                </div>
                                            </div>*@




                                    </div>

                                    <div class="col-lg-4">



                                    </div>

                                    <div class="col-lg-4 ">


                                        <div class="row MasterForm">
                                            <label for="PurchaseRef" class="col-sm-4 col-form-label">Ref No</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="SaleRef" name="SaleRef" value="@Model.SalesReturnMasterObj.SaleRef" readonly>
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="PurchaseDate" class="col-sm-4 col-form-label">Date</label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <span class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fas fa-calendar-alt"></i>
                                                        </span>
                                                    </span>

                                                    @if (!String.IsNullOrEmpty(Model.SalesReturnMasterObj.SaleDate))
                                                    {
                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="SaleDate" name="SaleDate" value="@Model.SalesReturnMasterObj.SaleDate" required> }
                                    else
                                    {
                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="SaleDate" name="SaleDate" value="@DateTime.Now.ToString("dd-MMM-yyyy")" required>}

                                                </div>
                                            </div>


                                        </div>

                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">Ref</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="Manualbook_ref" readonly name="Manualbook_ref" value="@Model.SalesReturnMasterObj.Manualbook_ref" placeholder="Manualbook_Ref">
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="Seller_ID" class="col-sm-4 col-form-label">Seller</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="Seller_ID" name="Seller_ID" READONLY asp-items="@(new SelectList( ViewBag.SellerList,"Partner_ID","PartnerName", Model.SalesReturnMasterObj.Seller_ID))">
                                                    <option>Select Seller</option>

                                                </select>

                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label class="col-sm-4 col-form-label" for="Chassis">Sale Ref</label>

                                            @*<div class="col-sm-8">
                                                    <select class="form-control" id="SaleMasterRef_ID" name="SaleMasterRef_ID" onchange="GetSaleRefDetail()" asp-items="@(new SelectList( ViewBag.SaleRef,"SaleMaster_ID","SaleRef", Model.SalesReturnMasterObj.SaleMasterRef_ID))" required>
                                                        <option value="0">Select Ref</option>

                                                    </select>
                                                </div>*@
                                            <div class="col-sm-4" style="padding-right:1px;">
                                                <input type="text" class="form-control" id="SaleRefS" name="SaleRefS">
                                                <input type="hidden" class="form-control" id="SaleMasterRef_ID" name="SaleMasterRef_ID" value="@Model.SalesReturnMasterObj.SaleMasterRef_ID">
                                            </div>
                                            <div class="col-sm-4" style="padding-left:1px; ">
                                                <button type="button" onclick="GetSaleRefDetail()" class=" btn btn-primary" style="width:100%;">Get &nbsp; <i class="fa fa-search"></i></button>
                                            </div>
                                        </div>


                                    </div>



                                </div>

                            </div>



                        </section>
                    </form>

                </div>

            </div>

            <div class="row m-2">
                <div class="col-lg-3">
                    <h2 class="card-title" style="line-height:normal;">Details </h2>
                </div>
                <div class="col-lg-6">
                    &nbsp;
                </div>
                <div class="col-lg-3 text-right">
                    @*<button class="btn btn-sm btn-primary" onclick="ShowDetails();">Add Details &nbsp;<i class="fas fa-plus"></i></button>*@


                </div>
            </div>

            <div id="SalesDetailSection">

                <table class="table table-responsive-md table-bordered mb-0">
                    <thead>
                        <tr>
                            <th>Delete</th>
                            <th>Item</th>
                            <th>Description</th>
                            <th>Unit Price</th>
                            <th>Sale QTY</th>
                            <th>Return QTY</th>



                            <th>Amount</th>







                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.SalesReturnDetailList != null)
                        {

                            ViewBag.SubTotal = 0;
                            foreach (pa_SalesDetails item in Model.SalesReturnDetailList)
                            {

                                ViewBag.Grand_Amount = item.Grand_Amount;
                                ViewBag.Grand_VATExp = item.Grand_VATExp;
                                ViewBag.Grand_TotalAmount = item.Grand_TotalAmount;
                                ViewBag.Total_Amt_inWords = item.Total_Amt_inWords;

            <tr>


                <td>

                    <a onclick="DeleteSalesDetail('@item.SalesDetails_ID');" data-toggle="modal" data-target="#DeleteSalesDetailModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ItemName)
                    <br />
                    <span class="small"> @Html.DisplayFor(modelItem => item.ItemCode)</span>

                </td>
                <td>@Html.DisplayFor(modelItem => item.ItemDesc)</td>
                <td>@Html.DisplayFor(modelItem => item.Unit_Price)</td>
                <td>
                    @Html.DisplayFor(modelItem => item.SaleQty) x
                    @Html.DisplayFor(modelItem => item.UM_ID)
                    <br />
                    @Html.DisplayFor(modelItem => item.LocationName)

                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Quantity) x
                    @Html.DisplayFor(modelItem => item.UM_ID)


                </td>

                <td>@Html.DisplayFor(modelItem => item.Total_Amount)</td>





            </tr>
}
    }
                    </tbody>

                </table>

                <div class="invoice-summary">

                    <div class="row">

                        <div class="col-lg-9 col-md-9">

                        </div>

                        <div class="col-lg-3 col-md-3 justify-content-end text-right">
                            <table class="table h6 text-dark">
                                <tbody>



                                    <tr class="b-top-0">
                                        <td colspan="2"> Sub Total:</td>
                                        <td class="text-right" id="SubTotal">@ViewBag.Grand_Amount</td>
                                    </tr>



                                    <tr class="h5">
                                        <td colspan="2">Total:</td>
                                        <td class="text-right" id="Total_Payable">@ViewBag.Grand_TotalAmount</td>
                                    </tr>



                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>

                <section class="card">
                    <div class="card-body">
                        <header class="clearfix">
                            <div class="row m-2">

                                <div class="col-lg-6">
                                    <h4 class="modal-title">Payment Details</h4>
                                </div>


                                <div class="col-lg-6 text-right">
                                    <button class="btn btn-sm btn-primary" data-toggle="modal" data-target="#AddNewPaymentDetailModal">Add New &nbsp;<i class="fas fa-plus"></i></button>

                                </div>




                            </div>
                        </header>
                        <div id="DepsitReturnDetailSection">

                            <table class="table table-responsive-md table-bordered mb-0">
                                <thead>
                                    <tr>
                                        <th>Edit/Delete</th>
                                        <th>On Account</th>

                                        <th>Description</th>
                                        <th>Pay Via</th>
                                        <th>Ref</th>
                                        <th>Amount</th>
                                        <th>VAT Charges</th>
                                        <th>Total Amount</th>

                                    </tr>
                                </thead>
                                <tbody>

                                    @if (Model.PaymentDetailList != null)
                                    {


                                        foreach (Pa_PaymentDetails_DAL item in Model.PaymentDetailList)
                                        {


                                            ViewBag.Vat_ExpTotal = item.VatExpTotal;
                                            ViewBag.AmountTotal = item.AmountTotal;
                                            ViewBag.Grand_Total = item.Grand_Total;


                        <tr>




                            <td>
                                <a onclick="UpdatePaymentDetailGeneral('@item.PaymentDetails_ID'	,'@item.DR_accountID', '@item.Amount', '@item.VAT_Rate', '@item.Currency_ID', '@item.Currency_Rate', '@item.Amount_Others',
	                                    '@item.CR_accountID', '@item.Description', '@item.trans_ref','@item.cheque_bank_name','@item.cheque_date', '@item.cheque_no', '@item.VAT_Exp', '@item.Total_Amount');" data-toggle="modal" data-target="#UpdateNewPaymentDetailModal"><img style="width: 20px;" src="~/Img/Icons/edit.png" /></a>
                                <a onclick="DeletePaymentDetail('@item.PaymentDetails_ID');" data-toggle="modal" data-target="#DeletePaymentDetailModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                            </td>

                            <td>@Html.DisplayFor(modelItem => item.DR_AccountName)</td>
                            <td>@Html.DisplayFor(modelItem => item.Description)</td>
                            <td>@Html.DisplayFor(modelItem => item.CR_AcountName)</td>
                            <td>@Html.DisplayFor(modelItem => item.trans_ref)</td>
                            <td>@Html.DisplayFor(modelItem => item.Amount)</td>
                            <td>
                                @Html.DisplayFor(modelItem => item.VAT_Exp)
                            </td>
                            <td>@Html.DisplayFor(modelItem => item.Total_Amount)</td>


                        </tr>
}
                }

                                </tbody>

                            </table>



                        </div>


                    </div>
                </section>

                <hr />





            </div>


            <div class="row">

                    <div class="col-sm-3"><span class="show-grid-block">Created at : <b>@Model.SalesReturnMasterObj.Created_at </b></span></div>
                    <div class="col-sm-3"><span class="show-grid-block">Created by :</span><b>@Model.SalesReturnMasterObj.Created_by</b></div>
                    <div class="col-sm-3"><span class="show-grid-block">Modified at :<b>@Model.SalesReturnMasterObj.Modified_at </b></span></div>
                    <div class="col-sm-3"><span class="show-grid-block">Modified by : <b>@Model.SalesReturnMasterObj.Modified_by </b></span></div>
                </div>



        </div>


    </div>
</section>









<!--Insert Payment detail model-->
<div class="modal fade bs-modal-lg in" id="AddNewPaymentDetailModal" tabindex="-1" role="dialog" aria-hidden="true" style=" padding-right: 17px;">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header" style="background: #f6f6f6;">


                <h4 class="modal-title">Payment Details</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>

            </div>

            <form method="post" data-ajax="true" data-ajax-success="mySuccessInsertPaymentDetail" data-ajax-failure="myFailureInsertPaymentDetail"
                  asp-area="" asp-controller="Sales" asp-action="InsertPaymentDetail" data-ajax-loading="" id="FormInsertPaymentDetail">

                <div class="modal-body">

                    <section class="card">

                        <div class="card-body">



                            <div class="form-body">
                                <div class="row">
                                    @if (Model.SalesReturnMasterObj.SaleMaster_ID != null)
                                    {
                        <input id="hdSaleMaster_ID" name="hdSaleMaster_ID" type="hidden" value="@Model.SalesReturnMasterObj.SaleMaster_ID" /> }
                    else

                    {

        <input id="hdSaleMaster_ID" name="hdSaleMaster_ID" type="hidden" value="0" />}
                                    <div class="col-md-3">
                                        <div class="form-group">

                                            <label class="control-label">Account*</label>


                                            <select class="form-control" id="RDR_accountID"   name="DR_accountID"  Readonly>
                                                <option value="132">SALE RETURN-ON HOLD</option>
                                            </select>

                                        

                                        </div>
                                    </div>
                                    <!--/span-->

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Amount*</label>
                                            <input type="number" id="RAmount" name="Amount" class="form-control" step="any" placeholder="Enter Amount" onblur="CalculateSumAmountPaymentSaleRetrun()" required>

                                        </div>
                                    </div>

                                    <!--/span-->

                                    <div class="col-md-3">
                                        <label class="control-label">Currency & Rate</label>
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-5 col-sm-5" style="padding:0; padding-left:12px;">
                                                    <select class="form-control" id="RCurrency_ID" name="Currency_ID" asp-items="@(new SelectList(ViewBag.CurrencyList,"Currency_ID","Currency_ShortName"))">
                                                        <option>SELECT</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-7 col-sm-7" style="padding:0;">
                                                    <input type="number" id="RCurrency_Rate" name="Currency_Rate" step="any" onblur="CalculateSumAmountPaymentSaleRetrun()" value="1" class="form-control" placeholder="Rate">

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Other Amount </label>
                                            <input type="text" id="RAmount_Others" name="Amount_Others" step="any" class="form-control" placeholder="Other Amount" readonly>

                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Pay Via*</label>

                                            <select class="form-control" id="CR_accountID" name="CR_accountID" asp-items="@(new SelectList( ViewBag.CR_AccountsList,"Account_ID","AccountName"))">
                                            </select>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label">Description</label>
                                            <input type="text" id="Description" name="Description" class="form-control" placeholder="Enter Description here">

                                        </div>
                                    </div>

                                    <!--/span-->
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Ref </label>

                                            <input type="text" id="trans_ref" name="trans_ref" class="form-control" value="@Model.SalesReturnMasterObj.SaleRef" placeholder="" readonly>


                                        </div>
                                    </div>
                                    <!--/span-->
                                </div>
                                <div class="row">

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Bank</label>
                                            <input type="text" id="cheque_bank_name" name="cheque_bank_name" class="form-control" placeholder="Bank">

                                        </div>
                                    </div>
                                    <!--/span-->

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Cheque Date </label>

                                            <div class="input-group">
                                                <span class="input-group-prepend">
                                                    <span class="input-group-text">
                                                        <i class="fas fa-calendar-alt"></i>
                                                    </span>
                                                </span>

                                                <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="cheque_date" name="cheque_date">

                                            </div>


                                        </div>
                                    </div>
                                    <!--/span-->

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Cheque # </label>
                                            <input type="text" id="cheque_no" name="cheque_no" class="form-control" placeholder="Cheque #">

                                        </div>
                                    </div>


                                    <div class="col-md-3 col-lg-3">
                                        <label class="control-label">VAT Charges </label>
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-5 col-sm-5" style="padding:0; padding-left:12px;">
                                                    <select class="form-control" id="RVAT_Rate" name="VAT_Rate" onchange="CalculateSumAmountPaymentSaleRetrun()">
                                                        <option value="0">0%</option>
                                                        <option value="5">5%</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-7 col-sm-7" style="padding:0;">
                                                    <input type="text" id="RVAT_Exp" name="VAT_Exp" step="any" class="form-control" placeholder="total" readonly>

                                                </div>
                                            </div>
                                        </div>
                                    </div>



                                </div>

                                <div class="row">
                                    <div class="col-md-3 col-lg-3">
                                        <div class="form-group">
                                            <label class="control-label">Total </label>
                                            <input type="text" id="RTotal_Amount" name="Total_Amount" step="any" class="form-control" placeholder="total" readonly>

                                        </div>
                                    </div>
                                </div>




                            </div>


                        </div>



                    </section>
                </div>

                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Add</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                </div>
            </form>

        </div>
    </div>
</div>


<!-- Modal Update payment detail-->
<div class="modal fade bs-modal-lg in" id="UpdateNewPaymentDetailModal" tabindex="-1" role="dialog" aria-hidden="true" style=" padding-right: 17px;">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header" style="background: #f6f6f6;">


                <h4 class="modal-title">Payment Details</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>

            </div>

            <form method="post" data-ajax="true" data-ajax-success="mySuccessUpdatePaymentDetail" data-ajax-failure="myFailureUpdatePaymentDetail"
                  asp-area="" asp-controller="Sales" asp-action="UpdatePaymentDetail" data-ajax-loading="" id="FormUpdatePaymentDetail">

                <div class="modal-body">

                    <section class="card">

                        <div class="card-body">



