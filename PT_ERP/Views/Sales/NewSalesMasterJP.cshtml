@using DAL
@using DAL.Models
@model DAL.oClasses.OSales

@{ //ViewData["Title"] = "NewSale";
                Layout = "~/Views/Shared/_LayoutTablePages.cshtml"; }


@{ string Form_StockMaster_ActionMethod = "";
                int? SaleMaster_ID = ViewBag.SalesMaster_ID;


                if (Model.SalesMasterObjJP != null && Model.SalesMasterObjJP.SaleMaster_ID > 0)
                {

                    Form_StockMaster_ActionMethod = "UpdateSalesMasterJPMaster";


                }
                else
                {
                    Form_StockMaster_ActionMethod = "InsertSalesMasterJPMaster";


                } }

}


<header class="page-header">
    <h2>Sales</h2>

    <div class="right-wrapper text-right">
        <ol class="breadcrumbs">
            <li>
                <a href="@ViewBag.Home_Dashboard">
                    <i class="fas fa-home"></i>
                </a>
            </li>
            <li><a asp-area="" asp-controller="Sales" asp-action="SalesListJP"><span>Sales</span></a></li>
            <li><a asp-area="" asp-controller="@ViewBag.BreadCumController" asp-action="@ViewBag.BreadCumAction"><span>New Sale Master</span></a></li>

        </ol>

        <a class="sidebar-right-toggle"></a>
    </div>
</header>






<div class="row" id="SuccessErrorSection">
    @if (TempData["Success"] != null || TempData["Error"] != null)
    {
<div class="col-lg-12">
    @if (TempData["Success"] != null)
    {
<div class="alert alert-info">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
    <strong>Success!</strong> @TempData["Success"] <a href="" class="alert-link"></a>
</div>}

    @if (TempData["Error"] != null)
    {
<div class="alert alert-danger">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
    <strong>Error!</strong> @TempData["Error"] <a href="" class="alert-link"></a>
</div>}
</div>}


</div>

<!-- start: page -->
<section class="card">
    <div class="card-body">
        <div class="invoice">
            <header class="clearfix">
                <div class="row">
                    <div class="col-sm-6 mt-0">
                        <h2 class="h4 mt-0 mb-2 text-dark font-weight-bold">Sale Master</h2>

                    </div>

                    <div class="col-sm-6 mt-0 text-right">

                        @if (String.IsNullOrEmpty(Model.SalesMasterObjJP.SaleStatus))
                        {
            <h4 class=" mt-0 mb-2 text-dark"><b>Status:</b> <label>Pending</label></h4> }
                        else
                        {
            <h4 class=" mt-0 mb-2 text-dark"><b>Status:</b> <label>@Model.SalesMasterObjJP.SaleStatus</label></h4>}
                    </div>


                </div>
            </header>

            <div class="row">
                <div class="col-lg-8">


                    @if (Model.SalesMasterObjJP.SaleMaster_ID != null)
                    {
                        if (Model.SalesMasterObjJP.SaleMaster_ID > 0)
                        {

        <button type="button" id="ButtonSaleMasterUpdate" class="btn-sm btn btn-success">Update &nbsp;<i class="fas fa-save"></i></button>

                            <button type="button" class="btn-sm btn btn-primary">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                                                <button type="button" class="btn-sm btn btn-warning" data-toggle="modal" data-target="#ChangeStatusStockModal" onclick="ChangeStatusStockMaster('@Model.SalesMasterObjJP.SaleMaster_ID','5');">Cancel &nbsp;<i class="fas fa-cross"></i></button>

                                                                    <button type="button" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#DeleteStockMasterModal" onclick="DeleteStockMaster('@Model.SalesMasterObjJP.SaleMaster_ID');">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>

                                                                                        <button type="button" class="btn-sm btn btn-info" id="print-btn">Print &nbsp;<i class="fas fa-print"></i></button>

                                                                                                            <a asp-controller="Sales" asp-action="SalesListJP" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a> }
                                                                                                                        }
                                                                                                                        else
                                                                                                                        {

                                                                                                            <button type="button" id="ButtonSaleMasterIssue" class="btn-sm btn btn-success">Issue &nbsp;<i class="fas fa-save"></i></button>

                                                                                                                            <button type="button" class="btn-sm btn btn-primary" disabled="disabled">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                                                                                                                                            <button type="button" class="btn-sm btn btn-warning" disabled="disabled">Cancel &nbsp;<i class="fas fa-cross"></i></button>
                                                                                                                                                            <button type="button" class="btn-sm btn btn-danger" disabled="disabled">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                                                                                                                                                                            <button type="button" class="btn-sm btn btn-info">Print &nbsp;<i class="fas fa-print"></i></button>

                                                                                                                                                                                            <a asp-controller="Sales" asp-action="SalesListJP" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a>}

                </div>

            </div>

            <div class="row">

                <div class="col">



                    <form method="post" asp-area="" asp-controller="Sales" asp-action="@Form_StockMaster_ActionMethod" id="formStockMaster">
                        @Html.AntiForgeryToken()

                        <section class="card">

                            <div class="card-body">



                                <div class="row form-group">
                                    <div class="col-lg-4">

                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">Customer</label>
                                            <div class="col-sm-8">
                                                <select class="form-control select2" id="CustomerName" name="CustomerName" onchange="GetCustomerDetail()" asp-items="@(new SelectList( ViewBag.CustomerMaster,"Customer_ID","CustomerName", Model.SalesMasterObjJP.Customer_ID))" required>
                                                </select>

                                                <input type="hidden" id="Sale_ID" name="Sale_ID" value="@SaleMaster_ID" />
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">Contact</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="CustomerContact" name="CustomerContact" value="@Model.SalesMasterObjJP.Customer_Contact" placeholder="Contact Number">
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="Address" class="col-sm-4 col-form-label">Address</label>
                                            <div class="col-sm-8">
                                                @*<textarea class="form-control" rows="1" id="Invoice_address" name="Invoice_address" placeholder="">@Model.SalesMasterObjJP.Invoice_address</textarea>*@
                                                <input type="text" class="form-control" id="CustomerAddress" name="CustomerAddress" value="@Model.SalesMasterObjJP.Invoice_address" placeholder="Customer Address">
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="Sale_trans_type" class="col-sm-4 col-form-label">Sale Type</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" asp-for="@Model.SalesMasterObjJP.Saletype" id="SalesType" name="SalesType">
                                                    <option selected="selected" value="Local">Local</option>
                                                    <option value="Export">Export</option>


                                                </select>

                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="Bank_to_Transfer_payment_ID" class="col-sm-4 col-form-label">Payment Bank</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="Bank_to_Transfer_payment_ID" name="Bank_to_Transfer_payment_ID" asp-items="@(new SelectList( ViewBag.BasicBanksList,"Bank_ID","BankName", Model.SalesMasterObjJP.Bank_to_Transfer_payment_ID))">
                                                    <option value="0">Select Bank</option>

                                                </select>

                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-4">

                                    </div>

                                    <div class="col-lg-4 ">


                                        <div class="row MasterForm">
                                            <label for="SaleRef" class="col-sm-4 col-form-label">Ref No</label>
                                            <div class="col-sm-8">
                                                @if (!String.IsNullOrEmpty(Model.SalesMasterObjJP.SaleRef))
                                                {
                                    <input type="text" class="form-control" id="Ref" name="Ref" value="@Model.SalesMasterObjJP.SaleRef" readonly> }
                                                else
                                                {
                                    <input type="text" class="form-control" id="Ref" name="Ref" value="@Model.SalesMasterRefJP.SaleRef" readonly>}
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="SaleDate" class="col-sm-4 col-form-label">Sale Date</label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <span class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fas fa-calendar-alt"></i>
                                                        </span>
                                                    </span>

                                                    @if (!String.IsNullOrEmpty(Model.SalesMasterObjJP.SaleDate))
                                                    {
                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="SaleDate" name="SaleDate" value="@Model.SalesMasterObjJP.SaleDate" required> }
                                                    else
                                                    {
                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="SaleDate" name="SaleDate" value="@DateTime.Now.ToString("dd-MMM-yyyy")" required>}

                                                </div>
                                            </div>


                                        </div>

                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">
                                                SupplierRef
                                            </label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="ManualRef" name="ManualRef" value="@Model.SalesMasterObjJP.Manualbook_ref" placeholder="Other Ref">
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="SaleDate" class="col-sm-4 col-form-label">Date In</label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <span class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fas fa-calendar-alt"></i>
                                                        </span>
                                                    </span>

                                                    @if (!String.IsNullOrEmpty(Model.SalesMasterObjJP.Date))
                                                    {
                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="Date" name="Date" value="@Model.SalesMasterObjJP.Date"> }
                                                    else
                                                    {
                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="Date" name="Date" value="@DateTime.Now.ToString("dd-MMM-yyyy")">}

                                                </div>
                                            </div>


                                        </div>

                                    </div>


                                </div>

                            </div>



                        </section>
                    </form>

                </div>

            </div>

            <div class="row m-2">
                <div class="col-lg-6">
                    <h2 class="card-title" style="line-height:normal;">Sale Details </h2>
                </div>
                <div class="col-lg-6 text-right">
                    <button class="btn btn-sm btn-primary" data-toggle="modal" data-target="#AddNewSaleDetailModal">Add New &nbsp;<i class="fas fa-plus"></i></button>


                </div>
            </div>

            <div id="StockDetailSection">

                <table class="table table-responsive-md table-bordered mb-0">
                    <thead>
                        <tr>
                            <th>Edit/Delete</th>
                            <th>Chassis</th>


                            <th>Price</th>

                            <th>Recylcle</th>
                            <th>Plate Fee</th>
                            <th>Auction Fee</th>
                            <th>Off. Charges</th>
                            <th>Total</th>




                        </tr>
                    </thead>
                    <tbody>



                        @if (Model.SalesDetailListJP != null)
                        {


                            ViewBag.TotalAmount = 0;

                            foreach (SaleDetailJP item in Model.SalesDetailListJP)
                            {




                                decimal TotalAmount = Math.Round(item.Amount, 2);
                                ViewBag.TotalAmount = Convert.ToDecimal(ViewBag.TotalAmount) + TotalAmount;



            <tr>


                <td>

                    <a onclick="UpdateStockmasterDetail('@item.SalesDetails_ID','@item.Chassis_no','@item.Sale_Ref', '@item.Price', '@item.PriceTax', '@item.PriceRate','@item.RecycleFee','@item.PlateFee','@item.AuctionFee','@item.AuctionFeeTax','@item.Amount','@item.OfficeCharges','@item.OfficeChargesTax','@item.Make','@item.MakeModelDescription','@item.Color','@item.Year');" data-toggle="modal" data-target="#UdpateSaleDetailModal"><img style="width: 20px;" src="~/Img/Icons/edit.png" /></a>
                    <a onclick="DeleteStockDetail(@item.SalesDetails_ID);" data-toggle="modal" data-target="#DeleteStockDetailModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                </td>
                <td>@Html.DisplayFor(modelItem => item.Chassis_no)</td>

                <td>
                    @Html.DisplayFor(modelItem => item.Price)<br />
                    <span style="font-size:10px;">   @Html.DisplayFor(modelItem => item.PriceTax)</span>
                </td>
                <td>@Html.DisplayFor(modelItem => item.RecycleFee)</td>

                <td>@Html.DisplayFor(modelItem => item.PlateFee)</td>
                <td>
                    @Html.DisplayFor(modelItem => item.AuctionFee)<br />
                    <span style="font-size:10px;">  @Html.DisplayFor(modelItem => item.AuctionFeeTax) </span>
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.OfficeCharges)<br />
                    <span style="font-size:10px;">  @Html.DisplayFor(modelItem => item.OfficeChargesTax) </span>
                </td>

                <td>@Html.DisplayFor(modelItem => item.Amount)</td>

            </tr>}
                        }



                    </tbody>

                </table>

                <div class="invoice-summary">

                    <div class="row">

                        <div class="col-lg-9 col-md-9">
                        </div>



                        <div class="col-lg-3 col-md-3 justify-content-end text-right">
                            <table class="table h6 text-dark">
                                <tfoot>

                                    <tr></tr>

                                    @if (Model.SaleDetailGrandTotals != null)
                                    {

                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Price Total :</strong></td>
                            <td class="text-right" id="Total_Payable">@Model.SaleDetailGrandTotals.TotalPrice</td>
                        </tr> }
                                    else
                                    {
                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Price Total :</strong></td>
                            <td class="text-right" id="Total_Payable">0</td>
                        </tr>}


                                    @if (Model.SaleDetailGrandTotals != null)
                                    {

                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Recycle Total :</strong></td>
                            <td class="text-right" id="Total_Payable">@Model.SaleDetailGrandTotals.TotalRecyclePrice</td>
                        </tr> }
                                    else
                                    {
                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Recycle Total :</strong></td>
                            <td class="text-right" id="Total_Payable">0</td>
                        </tr>}


                                    @if (Model.SaleDetailGrandTotals != null)
                                    {

                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Auction Fee Total:</strong></td>
                            <td class="text-right" id="Total_Payable">@Model.SaleDetailGrandTotals.TotalAuctionPrice</td>
                        </tr> }
                                    else
                                    {
                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Auction Fee Total:</strong></td>
                            <td class="text-right" id="Total_Payable">0</td>
                        </tr>}

                                    @if (Model.SaleDetailGrandTotals != null)
                                    {

                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Number Plate Total:</strong></td>
                            <td class="text-right" id="Total_Payable">@Model.SaleDetailGrandTotals.TotalPlatNaumberPrice</td>
                        </tr> }
                                    else
                                    {
                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Auction Fee Total:</strong></td>
                            <td class="text-right" id="Total_Payable">0</td>
                        </tr>}


                                    @if (Model.SaleDetailGrandTotals != null)
                                    {

                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Tax Amount:</strong></td>
                            <td class="text-right" id="Total_Payable">@Model.SaleDetailGrandTotals.TotalTax</td>
                        </tr> }
                                    else
                                    {
                        <tr class="h6" style="width:55%;">
                            <td colspan="2"><strong>Tax Amount:</strong></td>
                            <td class="text-right" id="Total_Payable">0</td>
                        </tr>}










                                    <tr class="h6" style="width:55%;">
                                        <td colspan="2"><strong>Grand Total:</strong></td>
                                        <td class="text-right" id="Total_Payable">@ViewBag.TotalAmount</td>
                                    </tr>



                                </tfoot>
                            </table>
                        </div>
                    </div>

                </div>

            </div>
            <hr />
            <div class="row">
                <div class="col-sm-3"><span class="show-grid-block">Created at : <b>@Model.SalesMasterObjJP.Created_at </b></span></div>
                <div class="col-sm-3"><span class="show-grid-block">Created by :</span><b>@Model.SalesMasterObjJP.Created_by</b></div>
                <div class="col-sm-3"><span class="show-grid-block">Modified at :<b>@Model.SalesMasterObjJP.Modified_at </b></span></div>
                <div class="col-sm-3"><span class="show-grid-block">Modified by : <b>@Model.SalesMasterObjJP.Modified_by </b></span></div>
            </div>



        </div>


    </div>
</section>




<!-- Modal Insert Sale detail -->
<div class="modal fade bs-modal-lg" id="AddNewSaleDetailModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background: #f6f6f6;">


                <h4 class="modal-title">Sale  Details</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>

            </div>

            <form method="post" data-ajax="true" data-ajax-success="mySuccessStockDetail" data-ajax-failure="myFailureStockDetail"
                  asp-area="" asp-controller="Sales" asp-action="InsertSalesMasterJPDetail" data-ajax-loading="" id="FormInsertStockDetail">

                <div class="modal-body">

                    <section class="card">

                        <div class="card-body">

                            <div class="form-body">

                                <div class="row">
                                    @if (Model.SalesMasterObjJP.SaleMaster_ID != null)
                                    {
                        <input id="hdSalesMaster_ID" name="hdSalesMaster_ID" type="hidden" value="@Model.SalesMasterObjJP.SaleMaster_ID" /> }
                                    else

                                    {

                        <input id="hdSalesMaster_ID" name="hdSalesMaster_ID" type="hidden" value="0" />}

                                    <!--/span-->
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Chassis No</label>
                                            <input type="text" id="Chassis" name="Chassis" value="" class="form-control" onblur="ValidateChassis();" placeholder="Enter Chassis">
                                            <label id="ChassisValidationMessage"></label>

                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-lg-3 col-md-3 ByChassisClass">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Make">Make</label>
                                            <input type="text" id="Make" name="Make" class="form-control" placeholder="Make" readonly />

                                        </div>
                                    </div>

                                    <div class="col-lg-3 col-md-3 ByChassisClass">
                                        <div class="form-group">
                                            <label class="col-form-label" for="ModelDesc">Model</label>
                                            <input type="text" id="ModelDesc" name="ModelDesc" class="form-control" placeholder="Model" readonly />

                                        </div>
                                    </div>

                                    <div class="col-lg-3 col-md-3 ByChassisClass">
                                        <div class="form-group">
                                            <label class="col-form-label" for="ModelYear">Year</label>
                                            <input type="text" id="ModelYear" name="ModelYear" class="form-control" placeholder="Year" readonly />

                                        </div>
                                    </div>


                                    <div class="col-lg-3 col-md-3 ByChassisClass">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Color">Color</label>
                                            <input type="text" id="Color" name="Color" class="form-control" placeholder="Color" readonly />

                                        </div>
                                    </div>

                                    <div class="col-md-3">

                                        <label class="control-label">Price </label>

                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-8" style="padding:0;">
                                                    <input type="text" id="txtSalePrice" name="Price" class="form-control Sum divideSalePrice" placeholder="Basic Price">
                                                </div>
                                                <div class="col-md-4" style="padding:0;">
                                                    <input type="text" id="txtSalePriceRate" name="PriceRate" value="1" class="form-control divideSalePrice" placeholder="Rate">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Price Tax (10%) </label>
                                            <input type="text" id="txtSalePriceTax" name="PriceTax" class="form-control Sum" placeholder="Price Tax">

                                        </div>
                                    </div>
                                    <!--/span-->


                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Recycle Fee </label>
                                            <input type="text" id="txtRecycleFee" name="RecycleFee" class="form-control Sum" value="0" placeholder="Recycle Fee">

                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Auction Fee and Tax (10%) </label>


                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-md-8" style="padding:0;">
                                                        <input type="text" id="txtauctionfee" name="auctionfee" class="form-control Sum divideauction" value="0" placeholder="Auction Fee" onblur="CalcTax()">
                                                    </div>
                                                    <div class="col-md-4" style="padding:0;">
                                                        <input type="text" id="txtauctionfeetax" name="auctionfeetax" class="form-control Sum" placeholder="Tax" readonly="readonly">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">NumberPlate Fee </label>
                                            <input type="text" id="txtNumberPlate" name="NumberPlate" class="form-control Sum" value="0" placeholder="NumberPlate">

                                        </div>
                                    </div>
                                    <!--/span-->



                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Office Charges&Tax (10%) </label>


                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-md-8" style="padding:0;">
                                                        <input type="text" id="txtofficecharge" name="OfficeCharges" class="form-control Sum divideauction" placeholder="Office Charge" value="0" onblur="CalcOffTax()">
                                                    </div>
                                                    <div class="col-md-4" style="padding:0;">
                                                        <input type="text" id="txtofficechargetax" name="OfficeChargesTax" class="form-control Sum" placeholder="Tax" readonly="readonly">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->


                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="control-label">Total Price </label>
                                            <input type="text" id="txtTotalPrice" class="form-control" placeholder="Total Price" readonly="readonly">

                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>

                    </section>
                </div>

                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Add</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                </div>
            </form>

        </div>
    </div>
</div>



<!-- Modal Update Sale Detail -->
<div class="modal fade bs-modal-lg" id="UdpateSaleDetailModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background: #f6f6f6;">


