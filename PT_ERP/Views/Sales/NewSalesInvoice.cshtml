@using DAL
@using DAL.Models
@model DAL.oClasses.OSales
@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration
@{ //ViewData["Title"] = "SalesInvoice";
    Layout = "~/Views/Shared/_LayoutTablePages.cshtml"; }


@{ string Form_SalesMaster_ActionMethod = "";
    int? SaleMaster_ID = ViewBag.SaleMaster_ID;


    if (Model.SaleMasterObj != null && Model.SaleMasterObj.SaleMaster_ID > 0)
    {

        Form_SalesMaster_ActionMethod = "UpdateSalesInvoiceMaster";


    }
    else
    {
        Form_SalesMaster_ActionMethod = "InsertSalesInvoiceMaster";


    }



    //----total values
    ViewBag.Grand_Amount = "0";
    ViewBag.Grand_VATExp = "0";
    ViewBag.Grand_TotalAmount = "0";
    ViewBag.Total_Amt_inWords = "";



    string req = "";
    req = Configuration.GetSection("AppSettings").GetSection("IsRequired").Value;
    string SalesReportName = "";
    SalesReportName = Configuration.GetSection("AppSettings").GetSection("SalesReportName").Value;
    string CompanyCode = Configuration.GetSection("AppSettings").GetSection("CompanyReportFolderCode").Value;
}






<header class="page-header">
    <h2>@ViewBag.SectionHeaderTitle</h2>

    <div class="right-wrapper text-right">
        <ol class="breadcrumbs">
            <li>
                <a href="@ViewBag.Home_Dashboard">
                    <i class="fas fa-home"></i>
                </a>
            </li>
            <li><a asp-area="" asp-controller="Purchases" asp-action="PurchaseList"><span>@ViewBag.SectionSub_HeaderMainTitle</span></a></li>
            <li><a asp-area="" asp-controller="@ViewBag.BreadCumController" asp-action="@ViewBag.BreadCumAction"><span>@ViewBag.SectionSub_HeaderTitle</span></a></li>

        </ol>

        <a class="sidebar-right-toggle"></a>
    </div>
</header>






<div class="row" id="SuccessErrorSection">
    @if (TempData["Success"] != null || TempData["Error"] != null)
    {
        <div class="col-lg-12">
            @if (TempData["Success"] != null)
            {
                <div class="alert alert-info">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Success!</strong> @TempData["Success"] <a href="" class="alert-link"></a>
                </div>}

            @if (TempData["Error"] != null)
            {
                <div class="alert alert-danger">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Error!</strong> @TempData["Error"] <a href="" class="alert-link"></a>
                </div>}
        </div>}


</div>

<!-- start: page -->
<section class="card">
    <div class="card-body">
        <div class="invoice">
            <header class="clearfix">
                <div class="row">
                    <div class="col-sm-6 mt-0">
                        <h2 class="h4 mt-0 mb-2 text-dark font-weight-bold">Sales Invoice</h2>

                    </div>

                    <div class="col-sm-6 mt-0 text-right">

                        @if (String.IsNullOrEmpty(Model.SaleMasterObj.SaleStatus))
                        {
                            <h4 class=" mt-0 mb-2 text-dark"><b>Status:</b> <label>Pending</label></h4> }
                        else
                        {
                            <h4 class=" mt-0 mb-2 text-dark"><b>Status:</b> <label>@Model.SaleMasterObj.SaleStatus</label></h4>}


                    </div>


                </div>
            </header>

            <div class="row">
                <div class="col-lg-8">
                    @if (Model.SaleMasterObj.SaleMaster_ID != null)
                    {
                        if (Model.SaleMasterObj.SaleMaster_ID > 0)
                        {

                    <button type="button" id="ButtonSaleMasterUpdate" class="btn-sm btn btn-success">Update &nbsp;<i class="fas fa-save"></i></button>

                    <button type="button" class="btn-sm btn btn-primary">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                    <button type="button" id="ChangeStatusMaster" class="btn-sm btn btn-warning" data-toggle="modal" data-target="#ChangeStatusSalesModal" onclick="ChangeStatusSalesMaster('@Model.SaleMasterObj.SaleMaster_ID', '5');">Cancel &nbsp;<i class="fas fa-cross"></i></button>

                    <button type="button" id="btnDelete" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#DeleteSalesMasterModal" onclick="DeleteSalesMaster('@Model.SaleMasterObj.SaleMaster_ID');">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                    <button type="button" id="btnApprove" class="btn-sm btn btn btn-dark" @ViewBag.ApprovalVisibility data-toggle="modal" data-target="#ChangeStatusSalesModal" onclick="ChangeStatusSalesMaster('@Model.SaleMasterObj.SaleMaster_ID', '2');">Approve &nbsp;<i class="fa fa-check"></i></button>
                            if (CompanyCode == "GL")
                            {

                                if (Model.SaleMasterObj.ApprovedBy > 0)
                                {
                    <button type="button" class="btn-sm btn btn-info" @ViewBag.canceled id="print-btn">Print &nbsp;<i class="fas fa-print"></i></button>
                    <button type="button" class="btn-sm btn btn-info" @ViewBag.canceled id="print-btn-agreement">Prn-Agr&nbsp;<i class="fas fa-print"></i></button> }
                            }
                            else
                            {

                    <button type="button" class="btn-sm btn btn-info" id="print-btn">Print &nbsp;<i class="fas fa-print"></i></button>
                    if(CompanyCode == "FM")
                    {

                    <button type="button" class="btn-sm btn btn-info" disabled="disabled" id="print-btn-agreement">Prn-Agr&nbsp;<i class="fas fa-print"></i></button>

                    }

                    else
                    {
                    <button type="button" class="btn-sm btn btn-info" id="print-btn-agreement">Prn-Agr&nbsp;<i class="fas fa-print"></i></button>

                    }

                    }


                    <button type="button" id="btnAttachment" class="btn-sm btn btn btn-default" @ViewBag.AttachmentVisibility style="background-color: #68889E; color:white; border-color:#2F3E48"
                            onclick="ShowAttachmentMaster('@Model.SaleMasterObj.SaleMaster_ID');">
                        Attachment &nbsp;<i class="fa fa-paperclip"></i>
                    </button>

                    <a asp-controller="Sales" asp-action="SalesInvoiceList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a> }
                    }
                    else
                    {
                    <button type="button" id="ButtonSaleMasterIssue" class="btn-sm btn btn-success">Issue &nbsp;<i class="fas fa-save"></i></button>

                    <button type="button" class="btn-sm btn btn-primary" disabled="disabled">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                    <button type="button" class="btn-sm btn btn-warning" disabled="disabled">Cancel &nbsp;<i class="fas fa-cross"></i></button>
                    <button type="button" class="btn-sm btn btn-danger" disabled="disabled">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                    <button type="button" class="btn-sm btn btn btn-dark" @ViewBag.ApprovalVisibility disabled="disabled">Approve &nbsp;<i class="fa fa-check"></i></button>
                    <button type="button" class="btn-sm btn btn-info" disabled="disabled">Print &nbsp;<i class="fas fa-print"></i></button>
                    <button type="button" class="btn-sm btn btn-info" disabled="disabled">Prn-Agr&nbsp;<i class="fas fa-print"></i></button>

                    <a asp-controller="Sales" asp-action="SalesInvoiceList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a>}
                </div>

            </div>

            <div class="row">

                <div class="col">



                    <form method="post" asp-area="" asp-controller="Sales" asp-action="@Form_SalesMaster_ActionMethod" id="formSalesMaster">
                        @Html.AntiForgeryToken()

                        <section class="card">

                            <div class="card-body">



                                <div class="row form-group">
                                    <div class="col-lg-4">

                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">Customer</label>
                                            <div class="col-sm-8">
                                                <select class="form-control select2" id="Customer_ID" name="Customer_ID" onchange="GetCustomerDetail()" asp-items="@(new SelectList( ViewBag.CustomerMaster,"Customer_ID","CustomerName", Model.SaleMasterObj.Customer_ID))" required>
                                                    <option value="0">Cash Customer</option>
                                                </select>
                                                <input type="hidden" id="SaleMaster_ID" name="SaleMaster_ID" value="@SaleMaster_ID" />
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">Name</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="Customer_Name" name="Customer_Name" value="@Model.SaleMasterObj.CustomerName" placeholder="Name">
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">Contact</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="Customer_Contact" name="Customer_Contact" value="@Model.SaleMasterObj.Customer_Contact" placeholder="Contact Number">
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="Address" class="col-sm-4 col-form-label">Address</label>
                                            <div class="col-sm-8">
                                                @*<textarea class="form-control" rows="1" id="Invoice_address" name="Invoice_address" placeholder="">@Model.SaleMasterObj.Invoice_address</textarea>*@
                                                <input type="text" class="form-control" id="Invoice_address" name="Invoice_address" value="@Model.SaleMasterObj.Invoice_address" placeholder="Customer Address">
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">CustomerTRN</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="CustomerTRN" name="CustomerTRN" value="@Model.SaleMasterObj.CustomerTRN" placeholder="Customer TRN">
                                            </div>
                                        </div>

                                        
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">Remarks</label>
                                            <div class="col-sm-8">
                                                <textarea type="text" class="form-control" id="Remarks" name="Remarks" placeholder="Remarks">@Model.SaleMasterObj.Remarks</textarea>
                                            </div>
                                        </div>


                                    </div>

                                    <div class="col-lg-4">
                                        <div class="row MasterForm">
                                            <label for="ExportTo_Destination_ID" class="col-sm-4 col-form-label">Destination</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="ExportTo_Destination_ID" name="ExportTo_Destination_ID" asp-items="@(new SelectList( ViewBag.ExportDestinations,"Destinations_ID","Destinations", Model.SaleMasterObj.ExportTo_Destination_ID))">
                                                    <option value="0"> Select Destination</option>

                                                </select>

                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="Agent_ID" class="col-sm-4 col-form-label">Port Exit</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="Port_of_Exit_ID" name="Port_of_Exit_ID" asp-items="@(new SelectList( ViewBag.PortOfExit,"PortOfExit_ID","PortOfExit", Model.SaleMasterObj.Port_of_Exit_ID))">
                                                    <option value="0">Select Port</option>

                                                </select>

                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="Sale_trans_type" class="col-sm-4 col-form-label">Sale Type</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" asp-for="@Model.SaleMasterObj.Sale_trans_type" id="Sale_trans_type" name="Sale_trans_type">
                                                    <option selected="selected" value="EXP">EXPORT</option>
                                                    <option value="IMP">IMPORT</option>
                                                    <option value="TRF">TRANSFER</option>

                                                </select>

                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="Showroom_ID" class="col-sm-4 col-form-label">Showroom</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="Showroom_ID" name="Showroom_ID" asp-items="@(new SelectList( ViewBag.ShowroomsList,"ShowRoom_ID","ShowRoomName", Model.SaleMasterObj.Showroom_ID))">
                                                    <option>Select Showroom</option>

                                                </select>

                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="ExporterCo_ID" class="col-sm-4 col-form-label">Export Co</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="ExporterCo_ID" name="ExporterCo_ID" asp-items="@(new SelectList( ViewBag.ExportCompanies,"Partner_ID","PartnerName", Model.SaleMasterObj.ExporterCo_ID))">

                                                    <option value="0"> Select Export Co</option>

                                                </select>

                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="Currency_ID" class="col-sm-4 col-form-label">Currency</label>
                                            <div class="col-sm-8">
                                                <div class="row">
                                                    <div class="col-sm-5">

                                                        <select class="form-control" id="Currency_ID" name="Currency_ID"
                                                                asp-items="@(new SelectList( ViewBag.CurrencyList,"Currency_ID","Currency_ShortName",(Model.SaleMasterObj.Currency_ID == 0 ? ViewBag.Default_Currency_ID : Model.SaleMasterObj.Currency_ID)))">
                                                        </select>
                                                    </div>
                                                    <div class="col-sm-6" style="padding:0;">
                                                        <input type="number" id="Currency_Rate" name="Currency_Rate" step="any" value="@(Model.SaleMasterObj.Curr_Rate==null ? ViewBag.Default_Curr_Rate : Model.SaleMasterObj.Curr_Rate)" required class="form-control" placeholder="Rate">
                                                    </div>
                                                </div>

                                            </div>
                                        </div>

                                    </div>

                                    <div class="col-lg-4 ">


                                        <div class="row MasterForm">
                                            <label for="PurchaseRef" class="col-sm-4 col-form-label">Ref No</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="SaleRef" name="SaleRef" value="@Model.SaleMasterObj.SaleRef" readonly>
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="PurchaseDate" class="col-sm-4 col-form-label">Date</label>
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    <span class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fas fa-calendar-alt"></i>
                                                        </span>
                                                    </span>

                                                    @if (!String.IsNullOrEmpty(Model.SaleMasterObj.SaleDate))
                                                    {
                                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="SaleDate" name="SaleDate" value="@Model.SaleMasterObj.SaleDate" required> }
                                                    else
                                                    {
                                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="SaleDate" name="SaleDate" value="@DateTime.Now.ToString("dd-MMM-yyyy")" required>}

                                                </div>
                                            </div>


                                        </div>

                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-4 col-form-label">Manual Ref</label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control" id="Manualbook_ref" name="Manualbook_ref" value="@Model.SaleMasterObj.Manualbook_ref" placeholder="Manualbook_Ref">
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="Seller_ID" class="col-sm-4 col-form-label">Seller</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="Seller_ID" name="Seller_ID" asp-items="@(new SelectList( ViewBag.SellerList,"Partner_ID","PartnerName", Model.SaleMasterObj.Seller_ID))">
                                                    <option>Select Seller</option>

                                                </select>

                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="Agent_ID" class="col-sm-4 col-form-label">Agent</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="Agent_ID" name="Agent_ID" asp-items="@(new SelectList( ViewBag.AgentsList,"Partner_ID","PartnerName", Model.SaleMasterObj.Agent_ID))">
                                                    <option>Select Agent</option>

                                                </select>

                                            </div>
                                        </div>


                                        <div class="row MasterForm hidden">
                                            <label for="Bank_to_Transfer_payment_ID" class="col-sm-4 col-form-label">Payment Bank</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="Bank_to_Transfer_payment_ID" name="Bank_to_Transfer_payment_ID" asp-items="@(new SelectList( ViewBag.BasicBanksList,"Bank_ID","BankName", Model.SaleMasterObj.Bank_to_Transfer_payment_ID))">
                                                    <option value="0">Select Bank</option>

                                                </select>

                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="shipping_co_ID" class="col-sm-4 col-form-label">Shipping Co</label>
                                            <div class="col-sm-8">
                                                <select class="form-control" id="shipping_co_ID" name="shipping_co_ID" asp-items="@(new SelectList( ViewBag.ShippingCompanies,"Partner_ID","PartnerName", Model.SaleMasterObj.shipping_co_ID))">
                                                    <option>Select Ship Co</option>

                                                </select>

                                            </div>
                                        </div>


                                    </div>


                                </div>
                            </div>


                        </section>
                    </form>

                </div>

            </div>

            <div class="row m-2">
                <div class="col-lg-6">
                    <h2 class="card-title" style="line-height:normal;">Sale Details </h2>
                </div>
                <div class="col-lg-6 text-right">
                    <button class="btn btn-sm btn-primary" id="btnNewAdd" data-toggle="modal" data-target="#AddNewSaleDetailModal">Add New &nbsp;<i class="fas fa-plus"></i></button>


                </div>
            </div>

            <div id="SalesDetailSection">

                <table class="table table-responsive-md table-bordered mb-0">
                    <thead>
                        <tr>
                            <th>Edit/Delete</th>
                            <th>Chassis</th>
                            <th>Make</th>
                            <th>Model</th>

                            <th>Color</th>

                            <th>Price</th>



                        </tr>
                    </thead>
                    <tbody>

                        @if (Model.SalesDetailList != null)
                        {


                            foreach (pa_SalesDetails item in Model.SalesDetailList.Where(m => m.SaleInvoiceType == "ByChassis"))
                            {

                                ViewBag.Grand_Amount = item.Grand_Amount;
                                ViewBag.Grand_VATExp = item.Grand_VATExp;
                                ViewBag.Grand_TotalAmount = item.Grand_TotalAmount;
                                ViewBag.Total_Amt_inWords = item.Total_Amt_inWords;
                                ViewBag.Total_Discount = item.Total_Discount;

                                <tr>


                                    <td>

                                        <a onclick="UpdateSalesDetailByChassis('@item.SalesDetails_ID', '@item.Chassis_No', '@item.Unit_Price','@item.VATRate', '@item.VATExp', '@item.Total_Amount' , '@item.Make', '@item.ModelDesciption', '@item.Discount', '@item.ModelYear', '@item.Color');" data-toggle="modal" data-target="#UdpateSaleDetailByChassisModal"><img style="width: 20px;" src="~/Img/Icons/edit.png" /></a>
                                        <a onclick="DeleteSalesDetail('@item.SalesDetails_ID');" data-toggle="modal" data-target="#DeleteSalesDetailModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                                    </td>
                                    <td>@Html.DisplayFor(modelItem => item.Chassis_No)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Make)</td>
                                    <td>@Html.DisplayFor(modelItem => item.ModelDesciption)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Color)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Unit_Price)</td>



                                </tr>
                            }
                        }


                    </tbody>

                </table>


                <table class="table table-responsive-md table-bordered mb-0 mt-3">
                    <thead>
                        <tr>
                            <th>Edit/Delete</th>
                            <th>Service Description</th>

                            <th>Price</th>



                        </tr>
                    </thead>
                    <tbody>

                        @if (Model.SalesDetailList != null)
                        {


                            foreach (pa_SalesDetails item in Model.SalesDetailList.Where(m => m.SaleInvoiceType == "ByService"))
                            {




                                <tr>
                                    <td>
                                        <a onclick="UpdateSalesDetailByServices('@item.SalesDetails_ID','@item.ItemDesc' ,'@item.Unit_Price', '@item.VATRate', '@item.VATExp', '@item.Total_Amount')" data-toggle="modal" data-target="#UdpateSaleDetailByServicesModal">
                                            <img style="width: 20px;" src="~/Img/Icons/edit.png" />
                                        </a>
                                        <a onclick="DeleteSalesDetail('@item.SalesDetails_ID');" data-toggle="modal" data-target="#DeleteSalesDetailModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                                    </td>
                                    <td>@Html.DisplayFor(modelItem => item.ItemDesc)</td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Unit_Price)

                                    </td>


                                </tr>
                            }
                        }

                    </tbody>

                </table>



                <div class="invoice-summary">

                    <div class="row">

                        <div class="col-lg-6 col-md-6">
                            <div class="form-group">
                                <br />
                                <label class="col-form-label" for="uu">Amounts</label>

                                <input type="text" id="uu" name="uuu" class="form-control" value="@ViewBag.Total_Amt_inWords" readonly />


                            </div>
                        </div>

                        <div class="col-lg-2 col-md-2">

                        </div>

                        <div class="col-lg-4 col-md-4 justify-content-end text-right">
                            <table class="table h6 text-dark">
                                <tbody>

                                    <tr class="b-top-0">
                                        <td colspan="2"> Sub Total:</td>
                                        <td class="text-right" id="SubTotal">@ViewBag.Grand_Amount</td>
                                    </tr>
                                    <tr>
                                        <td colspan="2">Discount:</td>
                                        <td class="text-right" id="Discount_ch">@ViewBag.Total_Discount</td>
                                    </tr>
                                    <tr>
                                        <td colspan="2">VAT Charge:</td>
                                        <td class="text-right" id="VAT_ChargeTotal">@ViewBag.Grand_VATExp</td>
                                    </tr>


                                    <tr class="h5">
                                        <td colspan="2">Total Sale Value:</td>
                                        <td class="text-right" id="Total_Payable">@ViewBag.Grand_TotalAmount</td>
                                    </tr>
                                    @if (Model.SaleMasterObj.SaleMaster_ID > 0)
                                    {
                                        <tr class="h5">
                                            <td colspan="2">Paid:</td>
                                            <td class="text-right" id="Paid_amt">@Model.SaleMasterBalanceObj.Paid_amt</td>
                                        </tr>
                                        <tr class="h5">
                                            <td colspan="2">Balance Due:</td>
                                            <td class="text-right" id="Bal_due">@Model.SaleMasterBalanceObj.Bal_due</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>
            </div>


            <div class="row m-2">
                <div class="col-lg-6">
                    <h2 class="card-title" style="line-height:normal;">Receipts Details </h2>
                </div>
                <div class="col-lg-6 text-right">

                    <button class="btn btn-sm btn-primary " id="btnSavePopUp" data-toggle="modal" data-target="#AddNewReceiptDetailModal">Add New &nbsp;<i class="fas fa-plus"></i></button>





                </div>
            </div>
            <div class="row">
                <div id="ReceiptDetailSection">
                    <table class="table table-responsive-sm table-bordered m-sm-0">
                        <thead>


                            <tr>
                                <th>Edit/Delete</th>
                                <th>On Account</th>
                                <th>Amount</th>
                                <th width="30%">Description</th>

                                <th>Recieved Mode</th>
                                <th>Ref</th>
                                <th>Type</th>
                                <th>Cheque Details</th>

                            </tr>
                        </thead>
                        <tbody>

                            @if (Model.SalesReceiptDetailsList != null)
                            {


                                foreach (pa_ReceiptDetails_DAL item in Model.SalesReceiptDetailsList)
                                {
                                    <tr style='color: @(item.transaction_status == "0" ? "green" : "black");'>
                                        <td>
                                            <a @(item.Receipttype == "RVS" ? "hidden" : "") onclick="UpdateReceiptDetailGeneral('@item.ReceiptDetail_ID','@item.DR_accountID','@item.Amount','@item.Currency_ID','@item.Currency_Rate','@item.other_curr_amount','@item.CR_accountID','@item.Description','@item.trans_ref','@item.Cheque_Bank_Name',
