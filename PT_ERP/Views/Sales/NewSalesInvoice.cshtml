        //---function for calculating amounts in the puchase detail pop up form insert

        function CalculateSumAmount() {



            var Unit_Price = 0;

            var VATRate = 5;
            var Discount = 0;
            var VATExp = 0;

            var Total_Amount = 0;



            Unit_Price = document.getElementById("Unit_Price").value;

            VATRate = document.getElementById("VATRate").value;

            VATExp = document.getElementById("VATExp").value;

            Total_Amount = document.getElementById("Total_Amount").value;
            Discount = document.getElementById("Discount").value;

            if (!Unit_Price) {
                Unit_Price = 0;
            }

            if (!VATRate) {
                VATRate = 5;

            }
            if (!VATExp) {
                VATExp = 0;
            }
            if (!Total_Amount) {
                Total_Amount = 1;
            }

            if (!Discount) {
                Discount = 0;
            }
            //---calculate sum vat exp
            VATExp = VATRate * Unit_Price / 100;
            $("#VATExp").val(VATExp);

            Total_Amount = (Number(Unit_Price) + Number(VATExp)) - Number(Discount);
            $("#Total_Amount").val(Total_Amount);



        }

