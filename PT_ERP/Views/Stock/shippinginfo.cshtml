@using DAL
@using DAL.Models
@model DAL.oClasses.OStock
@{
    ViewData["Title"] = "shippinginfo";
    Layout = "~/Views/Shared/_LayoutTablePages.cshtml";
}

@{
    string Form_formShipping_infoMaster_ActionMethod = "";
    int? Shipping_info_master_ID = ViewBag.Shipping_info_master_ID;


    if (Model.Shipping_infoMasterObj != null && Model.Shipping_infoMasterObj.Shipping_info_ID > 0)
    {

        Form_formShipping_infoMaster_ActionMethod = "UpdateShipping_infoMaster";


    }
    else
    {
        Form_formShipping_infoMaster_ActionMethod = "InsertShipping_infoMaster";


    }

}
<header class="page-header">
    <h2>Shipping Info</h2>

    <div class="right-wrapper text-right">
        <ol class="breadcrumbs">
            <li>
                <a href="@ViewBag.Home_Dashboard">
                    <i class="fas fa-home"></i>
                </a>
            </li>
            <li><a asp-area="" asp-controller="Stock" asp-action="Shipping_infoList"><span>Shipping_info List</span></a></li>
            <li><a asp-area="" asp-controller="@ViewBag.BreadCumController" asp-action="@ViewBag.BreadCumAction"><span>New Shipping_info</span></a></li>

        </ol>

        <a class="sidebar-right-toggle"></a>
    </div>
</header>






<div class="row" id="SuccessErrorSection">
    @if (TempData["Success"] != null || TempData["Error"] != null)
    {
        <div class="col-lg-12">
            @if (TempData["Success"] != null)
            {
                <div class="alert alert-info">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Success!</strong> @TempData["Success"] <a href="" class="alert-link"></a>
                </div>
            }

            @if (TempData["Error"] != null)
            {
                <div class="alert alert-danger">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Error!</strong> @TempData["Error"] <a href="" class="alert-link"></a>
                </div>
            }
        </div>
    }


</div>

<!-- start: page -->
<section class="card">
    <div class="card-body">
        <div class="invoice">
            <header class="clearfix">
                <div class="row">
                    <div class="col-sm-6 mt-0">
                        <h2 class="h4 mt-0 mb-2 text-dark font-weight-bold">Shipping Info </h2>

                    </div>



                </div>
            </header>

            <div class="row">
                <div class="col-lg-8">
                   @{ 

                       if (Model.Shipping_infoMasterObj.Shipping_info_ID > 0)
                       {

                    <button type="button" id="ButtonSaveMaster" class="btn-sm btn btn btn-success">Update &nbsp;<i class="fas fa-save"></i></button>

                    <button type="button" class="btn btn-sm btn-primary">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                    <button type="button" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#DeleteShipping_infoMasterNew" onclick="DeleteShipping_infoMaster('@Model.Shipping_infoMasterObj.Shipping_info_ID');">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                    <button class="btn-sm btn btn-warning" id="btnCancel" name="btnCancel" data-toggle="modal" data-target="#exampleModal3" onclick="CancelShipping_infoMaster('@Model.Shipping_infoMasterObj.Shipping_info_ID','5');">
                        Cancel
                        <i class="fas fa-cross"></i>
                    </button>
                    @*<button type="button" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#exampleModal1" >Delete &nbsp;<i class="fas fa-trash-alt"></i></button>*@
                    <button type="button" id="print-btn" class="btn-sm btn btn-info">Print &nbsp;<i class="fas fa-print"></i></button>


                    <a asp-controller="Stock" asp-action="Shipping_infoList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a>



                       }
                       else
                       {
                    <button type="button" id="ButtonIssue" class="btn-sm btn btn-success">Issue &nbsp;<i class="fas fa-save"></i></button>

                    <button type="button" class="btn-sm btn btn-primary" disabled="disabled">Edit  &nbsp;<i class="fas fa-edit"></i></button>

                    <button class="btn-sm btn btn-warning" id="btnCancel" name="btnCancel" data-toggle="modal" data-target="#exampleModal3" disabled="disabled">
                        Cancel
                        <i class="fas fa-cross"></i>
                    </button>

                    <button type="button" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#exampleModal1" disabled="disabled">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                    <button type="button" class="btn-sm btn btn-info" disabled="disabled">Print &nbsp;<i class="fas fa-print"></i></button>




                    <a asp-controller="Stock" asp-action="Shipping_infoList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a>
                           }

                       }



                </div>

            </div>

            <div class="row">

                <div class="col">



