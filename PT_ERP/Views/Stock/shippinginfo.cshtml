                    </form>
                </div>

            </div>

            <div class="row m-1">
                <div class="col-lg-3">
                    <h2 class="card-title" style="line-height:normal;">Shipping_info Details </h2>
                </div>


                <div class="col-lg-9 col-md-9 text-right">
                    <div class="row">
                        <div class="col-lg-6 text-left">
                            <label for="Sale_ID" class="col-sm-3 col-form-label">Sale Ref</label>
                            <div class="col-sm-12">
                                <select class="form-control" id="Sale_refDrop" name="Sale_refDrop" asp-items="@(new SelectList( ViewBag.SaleRefList,"SaleMaster_ID","SaleRef"))">
                                </select>
                            </div>

                        </div>
                        


                    </div>
                </div>
            </div>

            <div id="PaymentDetailSection">

                <table class="table table-responsive-md table-bordered mb-0">
                    <thead>
                        <tr>
                            <th>Edit/Delete</th>
                            <th>Chassis</th>
                            <th>Make</th>
                            <th>Shipping</th>
                            


                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.Shipping_infoDetailList != null)
                        {

                            ViewBag.SubTotal = 0;
                            ViewBag.Vat_ExpTotal = 0;
                            ViewBag.DiscountTotal = 0;
                            ViewBag.Grand_Total = 0;


                            foreach (pa_Shipping_Info_details item in Model.Shipping_infoDetailList)
                            {



                                <tr>
                                    <td>
                                        <a onclick="UpdateShipping_infoDetail('@item.ID', '@item.Chassis_no', '@item.make_ID', '@item.model_description','@item.Berth_Carry_Charges','@item.Berth_Carry_ChargesTax','@item.TotalCharges');"><img style="width: 20px;" src="~/Img/Icons/edit.png" /></a>
                                        
                                        <a onclick="DeleteShipping_infoDetail('@item.ID');" data-toggle="modal" data-target="#DeleteShipping_infoDetailModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                                    </td>
                                    <td>@Html.DisplayFor(modelItem => item.Chassis_no)</td>
                                    <td>@Html.DisplayFor(modelItem => item.make)</td>
                                    <td>@Html.DisplayFor(modelItem => item.TotalCharges)</td>
                                 

                                </tr>
                            }
                        }




                    </tbody>

                </table>


                <div class="invoice-summary">

                    <div class="row">

                        <div class="col-lg-9 col-md-9">

                        </div>

                        <div class="col-lg-3 col-md-3 justify-content-end text-right">
