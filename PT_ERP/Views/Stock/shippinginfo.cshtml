                                                    <option>Select PortTo</option>
                                                </select>


                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-3 col-form-label">
                                                Vessel/Voy
                                            </label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" id="Vessel_Name" name="Vessel_Name" value="@Model.Shipping_infoMasterObj.Vessel_Name">
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-3 col-form-label">
                                                Frieght
                                            </label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" id="txtBerth" name="txtBerth" value="@Model.Shipping_infoMasterObj.Berth">
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="Shipping_infoDate" class="col-sm-3 col-form-label">BerthDT</label>
                                            <div class="col-sm-9">
                                                <div class="input-group">
                                                    <span class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fas fa-calendar-alt"></i>
                                                        </span>
                                                    </span>
                                                    @if (Model.Shipping_infoMasterObj.Berth_in_date != null)
                                                    {
                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="Berth_in_date" name="Berth_in_date" value="@Model.Shipping_infoMasterObj.Berth_in_date"> }
                                                    else
