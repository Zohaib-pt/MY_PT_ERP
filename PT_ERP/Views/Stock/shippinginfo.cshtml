                                <tbody>
                                    @{
                                        if (Model.Shipping_infoMasterObj != null)
                                        {
                                            <tr class="b-top-0">
                                                <td colspan="2"> BerthCarry:</td>
                                                <td class="text-right" id="Price">@Model.Shipping_infoMasterObj.Berth_Carry_Charges</td>
                                            </tr>
                                        }

                                        else
                                        {
                                            <tr class="b-top-0">
                                                <td colspan="2"> BerthCarry:</td>
                                                <td class="text-right" id="Price">0</td>
                                            </tr>
                                        }
                                        if (Model.Shipping_infoMasterObj != null)
                                        {
                                            <tr>
                                                <td colspan="2">Total:</td>
                                                <td class="text-right" id="TotalTax">@Model.Shipping_infoMasterObj.Berth_Carry_Charges</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>
                                                <td colspan="2">Total:</td>
                                                <td class="text-right" id="TotalTax">0</td>
                                            </tr>
                                        }
                                        if (Model.Shipping_infoMasterObj != null)
                                        {
                                            <tr>
                                                <td colspan="2">Paid:</td>
                                                <td class="text-right" id="Paid">@Model.Shipping_infoMasterObj.Paid</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>
                                                <td colspan="2">Paid:</td>
                                                <td class="text-right" id="Paid">0</td>
                                            </tr>
                                        }
                                        if (Model.Shipping_infoMasterObj != null)
                                        {
                                            <tr>
                                                <td colspan="2">Balance:</td>
                                                <td class="text-right" id="Balance">@Model.Shipping_infoMasterObj.Balance</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>
                                                <td colspan="2">Balance:</td>
                                                <td class="text-right" id="Balance">0</td>
                                            </tr>
                                        }
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>

            </div>


            <hr />
            <div class="row">
                <div class="col-sm-3"><span class="show-grid-block">Created at : <b>@Model.Shipping_infoMasterObj.created_at </b></span></div>
                <div class="col-sm-3"><span class="show-grid-block">Created by :</span><b>@Model.Shipping_infoMasterObj.created_by</b></div>
                <div class="col-sm-3"><span class="show-grid-block">Modified at :<b>@Model.Shipping_infoMasterObj.last_updated_at</b></span></div>
                <div class="col-sm-3"><span class="show-grid-block">Modified by : <b>@Model.Shipping_infoMasterObj.last_updated_by </b></span></div>
            </div>






        </div>


    </div>
</section>




<!-- Modal Insert task -->
<div class="modal fade bs-modal-lg" id="AddNewShipping_infoModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">

            <div class="modal-header" style="background: #f6f6f6;">


                <h4 class="modal-title">Shipping_Info Details</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>


            </div>



            <form method="post" data-ajax="true" data-ajax-complete="myComplete" data-ajax-success="mySuccess" data-ajax-failure="myFailure"
                  asp-area="" asp-controller="Stock" asp-action="InsertShipping_infoDetail" data-ajax-loading="" id="FormInsertShipping_infoDetail">

                <div class="modal-body">

                    <section class="card">

                        <div class="card-body">



                            <div class="row form-group">
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Make_ID">Chassis No*</label>



                                        <input type="text" id="Chassis" name="Chassis" onblur="ValidateChassis()" class="form-control" placeholder="Enter chassis" required />
                                        <label id="ChassisValidationMessage"></label>

                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Make_ID">Make*</label>



                                        <select class="form-control" id="Make_ID" name="Make_ID" asp-items="@(new SelectList( ViewBag.MakeList,"Make_ID","Make"))" onchange="GetSubModel(this);" required>
                                            <option>Select Make</option>
                                        </select>



                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Model_ID">Model*</label>

                                        <select class="form-control MakeModelClass" id="Model_ID" name="Model_ID" required>
                                            <option value="">Select Model</option>

                                        </select>

                                    </div>
                                </div>

                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="control-label">Berth (c) Charges & Tax </label>


                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-8" style="padding:0;">
                                                    <input type="number" id="txtShippingCharges" name="Berth_Carry_Charges" class="form-control" placeholder="Shipping" onblur="CalcTotalChargesInsert()">
                                                </div>
                                                <div class="col-md-4" style="padding:0;">
                                                    <input type="number" id="txtShippingChargestax" name="Berth_Carry_ChargesTax" class="form-control" placeholder="Tax" onblur="CalcTotalChargesInsert()">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                 


                                <div class="col-lg-4">

                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <label class="control-label">Total </label>
                                            <input type="text" id="txtTotalPrice" name="txtTotalPrice" class="form-control" placeholder="Total" readonly>

                                        </div>
                                    </div>







                                </div>

                            </div>






                        </div>



                    </section>
                </div>

