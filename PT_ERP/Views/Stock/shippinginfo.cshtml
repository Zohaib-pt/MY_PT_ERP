                                        </div>


                                        <div class="row MasterForm">
                                            <label for="Shipping_infoDate" class="col-sm-3 col-form-label">Invoice Date</label>
                                            <div class="col-sm-9">
                                                <div class="input-group">
                                                    <span class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fas fa-calendar-alt"></i>
                                                        </span>
                                                    </span>
                                                    @if (Model.Shipping_infoMasterObj.Invoice_Date != null)
                                                    {
                                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="txtInvDate" name="txtInvDate" value="@Model.Shipping_infoMasterObj.trans_Date"  readonly>

                                                    }
                                                    else
