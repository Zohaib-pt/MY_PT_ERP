        function CalculateSumAmount() {

            var Quanity = 0;
            var UnitPrice = 0;
            var Amount = 0;
            var Amount_Others = 0;
            var Currency_Rate = 0;

            var resultTotalAmount = 0;
            var resultTotalOtherAmount = 0;

            Quanity = document.getElementById("Quantity").value;
            UnitPrice = document.getElementById("Unit_Price").value;
            Amount = document.getElementById("Amount").value;
            Amount_Others = document.getElementById("Amount_Others").value;
            Currency_Rate = document.getElementById("Currency_Rate").value;

            if (!Quanity) {
                Quanity = 0;
            }
            if (!UnitPrice) {
                UnitPrice = 0;
            }
            if (!Amount) {
                Amount = 0;
            }
            if (!Amount_Others) {
                Amount_Others = 0;
            }
            if (!Currency_Rate) {
                Currency_Rate = 1;
            }

            //---calculate Amount
            resultTotalAmount = Quanity * UnitPrice;
            $("#Amount").val(resultTotalAmount);

            //--Calculate Other Amount

            resultTotalOtherAmount = Currency_Rate * resultTotalAmount;
            $("#Amount_Others").val(resultTotalOtherAmount);


        }

