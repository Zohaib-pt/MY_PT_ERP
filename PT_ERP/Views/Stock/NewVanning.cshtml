@using DAL
@using DAL.Models
@model DAL.oClasses.OStock
@{
    ViewData["Title"] = "NewVanning";
    Layout = "~/Views/Shared/_LayoutTablePages.cshtml";
}

@{
    string Form_formVanningMaster_ActionMethod = "";
    int? vanning_master_ID = ViewBag.vanning_master_ID;


    if (Model.VanningMasterObj != null && Model.VanningMasterObj.vanning_Master_ID > 0)
    {

        Form_formVanningMaster_ActionMethod = "UpdateVanningMaster";


    }
    else
    {
        Form_formVanningMaster_ActionMethod = "InsertVanningMaster";


    }

}
<header class="page-header">
    <h2>Vannings</h2>

    <div class="right-wrapper text-right">
        <ol class="breadcrumbs">
            <li>
                <a href="index.html">
                    <i class="fas fa-home"></i>
                </a>
            </li>
            <li><a asp-area="" asp-controller="Stock" asp-action="VanningList"><span>Vanning List</span></a></li>
            <li><a asp-area="" asp-controller="@ViewBag.BreadCumController" asp-action="@ViewBag.BreadCumAction"><span>New Vanning</span></a></li>

        </ol>

        <a class="sidebar-right-toggle"></a>
    </div>
</header>






<div class="row" id="SuccessErrorSection">
    @if (TempData["Success"] != null || TempData["Error"] != null)
    {
        <div class="col-lg-12">
            @if (TempData["Success"] != null)
            {
                <div class="alert alert-info">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Success!</strong> @TempData["Success"] <a href="" class="alert-link"></a>
                </div>
            }

            @if (TempData["Error"] != null)
            {
                <div class="alert alert-danger">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Error!</strong> @TempData["Error"] <a href="" class="alert-link"></a>
                </div>
            }
        </div>
    }


</div>

<!-- start: page -->
<section class="card">
    <div class="card-body">
        <div class="invoice">
            <header class="clearfix">
                <div class="row">
                    <div class="col-sm-6 mt-0">
                        <h2 class="h4 mt-0 mb-2 text-dark font-weight-bold">Vanning </h2>

                    </div>



                </div>
            </header>

            <div class="row">
                <div class="col-lg-8">
                    @if (Model.VanningMasterObj.vanning_Master_ID != null)
                    {
                        if (Model.VanningMasterObj.vanning_Master_ID > 0)
                        {

                            <button type="button" id="ButtonSaveMaster" class="btn-sm btn btn btn-success">Update &nbsp;<i class="fas fa-save"></i></button>

                            <button type="button" class="btn btn-sm btn-primary">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                            <button type="button" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#DeleteVanningMasterNew" onclick="DeleteVanningMaster('@Model.VanningMasterObj.vanning_Master_ID');">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                            <button class="btn-sm btn btn-warning" id="btnCancel" name="btnCancel" data-toggle="modal" data-target="#exampleModal3" onclick="CancelVanningMaster('@Model.VanningMasterObj.vanning_Master_ID','5');">
                                Cancel
                                <i class="fas fa-cross"></i>
                            </button>
                            @*<button type="button" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#exampleModal1" >Delete &nbsp;<i class="fas fa-trash-alt"></i></button>*@
                            <button type="button" id="print-btn" class="btn-sm btn btn-info">Print &nbsp;<i class="fas fa-print"></i></button>


                            <a asp-controller="Stock" asp-action="VanningList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a>



                        }
                        else
                        {
                            <button type="button" id="ButtonIssue" class="btn-sm btn btn-success">Issue &nbsp;<i class="fas fa-save"></i></button>

                            <button type="button" class="btn-sm btn btn-primary" disabled="disabled">Edit  &nbsp;<i class="fas fa-edit"></i></button>

                            <button class="btn-sm btn btn-warning" id="btnCancel" name="btnCancel" data-toggle="modal" data-target="#exampleModal3" disabled="disabled">
                                Cancel
                                <i class="fas fa-cross"></i>
                            </button>

                            <button type="button" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#exampleModal1" disabled="disabled">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                            <button type="button" class="btn-sm btn btn-info" disabled="disabled">Print &nbsp;<i class="fas fa-print"></i></button>




                            <a asp-controller="Stock" asp-action="VanningList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a>
                        }

                    }
                    else
                    {
                        <button type="button" id="ButtonIssue" class="btn-sm btn btn-success">Issue &nbsp;<i class="fas fa-save"></i></button>

                        <button type="button" class="btn-sm btn btn-primary" disabled="disabled">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                        <button class="btn-sm btn btn-warning" id="btnCancel" name="btnCancel" data-toggle="modal" data-target="#exampleModal3" disabled="disabled">
                            Cancel
                            <i class="fas fa-cross"></i>
                        </button>
                        <button type="button" class="btn-sm btn btn-danger" disabled="disabled">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                        <button type="button" class="btn-sm btn btn-info" disabled="disabled">Print &nbsp;<i class="fas fa-print"></i></button>




                        <a asp-controller="Stock" asp-action="VanningList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a>
                    }


                </div>

            </div>

            <div class="row">

                <div class="col">



                    <form method="post" asp-area="" asp-controller="Stock" asp-action="@Form_formVanningMaster_ActionMethod" id="formVanningMaster">
                        @Html.AntiForgeryToken()

                        <section class="card">

                            <div class="card-body">



                                <div class="row form-group">
                                    <div class="col-lg-4">

                                        <div class="row MasterForm">
                                            <label for="Vendor_ID" class="col-sm-3 col-form-label">Supplier</label>
                                            <div class="col-sm-9">
                                                <select class="form-control" id="Vendor_ID" name="Vendor_ID" onchange="AddNewVendor(this)" asp-items="@(new SelectList(ViewBag.VendorMaster, "Partner_ID", "PartnerName", Model.VanningMasterObj.Vendor_ID))" required>
                                                    <option>Select Vendor</option>
                                                    <option value="NewVendor">New Vendor A/C</option>
                                                </select>

                                                <input type="hidden" id="vanning_master_ID" name="vanning_master_ID" value="@vanning_master_ID" />
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-3 col-form-label">Comments</label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" id="Comments" name="Comments" value="@Model.VanningMasterObj.Comments">
                                            </div>
                                        </div>
                                        @*<div class="row MasterForm">
                                                <label for="Address" class="col-sm-3 col-form-label">Invoice Ref</label>
                                                <div class="col-sm-9">
                                                    <textarea class="form-control" rows="2" id="vanning_ref" name="vanning_ref" placeholder="">@Model.VanningMasterObj.Vendor_Address</textarea>
                                                </div>
                                            </div>*@



                                    </div>
                                    <div class="col-lg-4">

                                    </div>

                                    <div class="col-lg-4 ">

                                        <div class="row MasterForm">
                                            <label for="VanningRef" class="col-sm-3 col-form-label">Ref No</label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" id="vanning_ref" name="vanning_ref" value="@Model.VanningMasterObj.vanning_ref" readonly>
                                                <input type="hidden" id="htemp_ID" name="htemp_ID" value="@ViewBag.VanningRef" />
                                            </div>
                                        </div>


                                        <div class="row MasterForm">
                                            <label for="VanningDate" class="col-sm-3 col-form-label">Date</label>
                                            <div class="col-sm-9">
                                                <div class="input-group">
                                                    <span class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fas fa-calendar-alt"></i>
                                                        </span>
                                                    </span>
                                                    @if (Model.VanningMasterObj.Date != null)
                                                    {
                                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="Date" name="Date" value="@Model.VanningMasterObj.Date" required>

                                                    }
                                                    else
                                                    {
                                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="Date" name="Date" value="@DateTime.Now.ToString("dd-MMM-yyyy")" required>

                                                    }

                                                </div>
                                            </div>


                                        </div>





                                        <div class="row MasterForm">
                                            <label for="Discount" class="col-sm-3 col-form-label">Other Ref</label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" id="trans_Ref" name="trans_Ref" value="@Model.VanningMasterObj.trans_ref">
                                            </div>
                                        </div>
                                    </div>


                                </div>

                            </div>



                        </section>
                    </form>
                </div>

            </div>

            <div class="row m-1">
                <div class="col-lg-3">
                    <h2 class="card-title" style="line-height:normal;">Vanning Details </h2>
                </div>


                <div class="col-lg-9 col-md-9 text-right">
                    <div class="row">
                        <div class="col-lg-6 text-left">
                            <label for="Sale_ID" class="col-sm-3 col-form-label">Sale Ref</label>
                            <div class="col-sm-12">
                                <select class="form-control" id="Sale_refDrop" name="Sale_refDrop" asp-items="@(new SelectList(ViewBag.SaleRefList, "SaleMaster_ID", "SaleRef"))">
                                </select>
                            </div>

                        </div>
                  


                    </div>
                </div>
            </div>

            <div id="PaymentDetailSection">

                <table class="table table-responsive-md table-bordered mb-0">
                    <thead>
                        <tr>
                            <th>Edit/Delete</th>
                            <th>Chassis</th>
                            <th>Make</th>
                            <th>Make Model</th>
                            <th>Amount</th>


                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.VanningDetailList != null)
                        {

                            ViewBag.SubTotal = 0;
                            ViewBag.Vat_ExpTotal = 0;
                            ViewBag.DiscountTotal = 0;
                            ViewBag.Grand_Total = 0;


                            foreach (pa_Vanning_Details item in Model.VanningDetailList)
                            {




                                <tr>
                                    <td>
                                        <a onclick="UpdateVanningDetail('@item.ID');"><img style="width: 20px;" src="~/Img/Icons/edit.png" /></a>
                                        <a onclick="DeleteVanningDetail('@item.ID');" data-toggle="modal" data-target="#DeleteVanningDetailModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                                    </td>
                                    <td>@Html.DisplayFor(modelItem => item.Chassis_no)</td>
                                    <td>@Html.DisplayFor(modelItem => item.make)</td>
                                    <td>@Html.DisplayFor(modelItem => item.model_description)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Total_Amount)</td>

                                </tr>
                            }
                        }




                    </tbody>

                </table>


                <div class="invoice-summary">

                    <div class="row">

                        <div class="col-lg-9 col-md-9">

                        </div>

                        <div class="col-lg-3 col-md-3 justify-content-end text-right">
                            <table class="table h6 text-dark">
                                <tbody>

                                    @{
                                        if (Model.VanningMasterObj != null)
                                        {
                                            <tr class="b-top-0">
                                                <td colspan="2"> Price:</td>
                                                <td class="text-right" id="Price">@Model.VanningMasterObj.Amount</td>
                                            </tr>
                                        }

                                        else
                                        {
                                            <tr class="b-top-0">
                                                <td colspan="2"> Price:</td>
                                                <td class="text-right" id="Price">0</td>
                                            </tr>
                                        }

                                        if (Model.VanningMasterObj != null)
                                        {
                                            <tr>
                                                <td colspan="2">Total Tax:</td>
                                                <td class="text-right" id="TotalTax">@Model.VanningMasterObj.Tax_Amount</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>
                                                <td colspan="2">Total Tax:</td>
                                                <td class="text-right" id="TotalTax">0</td>
                                            </tr>
                                        }

                                        if (Model.VanningMasterObj != null)
                                        {
                                            <tr>
                                                <td colspan="2">Total:</td>
                                                <td class="text-right" id="Total">@Model.VanningMasterObj.Total_Amount</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>
                                                <td colspan="2">Total:</td>
                                                <td class="text-right" id="Total">0</td>
                                            </tr>
                                        }


                                        if (Model.VanningMasterObj != null)
                                        {
                                            <tr>
                                                <td colspan="2">Paid:</td>
                                                <td class="text-right" id="Paid">@Model.VanningMasterObj.Paid</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>
                                                <td colspan="2">Paid:</td>
                                                <td class="text-right" id="Paid">0</td>
                                            </tr>
                                        }

                                        if (Model.VanningMasterObj != null)
                                        {
                                            <tr>
                                                <td colspan="2">Balance:</td>
                                                <td class="text-right" id="Balance">@Model.VanningMasterObj.Balance</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>
                                                <td colspan="2">Balance:</td>
                                                <td class="text-right" id="Balance">0</td>
                                            </tr>
 }

                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>

            </div>


            <hr />
            <div class="row">
                <div class="col-sm-3"><span class="show-grid-block">Created at : <b>@Model.VanningMasterObj.Created_at </b></span></div>
                <div class="col-sm-3"><span class="show-grid-block">Created by :</span><b>@Model.VanningMasterObj.Created_By</b></div>
                <div class="col-sm-3"><span class="show-grid-block">Modified at :<b>@Model.VanningMasterObj.Last_Updated_at </b></span></div>
                <div class="col-sm-3"><span class="show-grid-block">Modified by : <b>@Model.VanningMasterObj.Last_Updated_by </b></span></div>
            </div>






        </div>


    </div>
</section>




<!-- Modal Insert task -->
<div class="modal fade bs-modal-lg" id="AddNewVanningModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">

            <div class="modal-header" style="background: #f6f6f6;">


                <h4 class="modal-title">Stock Details</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>


            </div>



            <form method="post" data-ajax="true" data-ajax-complete="myComplete" data-ajax-success="mySuccess" data-ajax-failure="myFailure"
                  asp-area="" asp-controller="Stock" asp-action="InsertVanningDetail" data-ajax-loading="" id="FormInsertVanningDetail">

                <div class="modal-body">

                    <section class="card">

                        <div class="card-body">



                            <div class="row form-group">
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Make_ID">Chassis No*</label>



                                        <input type="text" id="Chassis" name="Chassis" onblur="ValidateChassis()" class="form-control" placeholder="Enter chassis" required />
                                        <label id="ChassisValidationMessage"></label>

                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Make_ID">Make*</label>



                                        <select class="form-control" id="Make_ID" name="Make_ID" asp-items="@(new SelectList( ViewBag.MakeList,"Make_ID","Make"))" onchange="GetSubModel(this);" required>
                                            <option>Select Make</option>
                                        </select>



                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Model_ID">Model*</label>

                                        <select class="form-control MakeModelClass" id="Model_ID" name="Model_ID" required>
                                            <option value="">Select Model</option>

                                        </select>

                                    </div>
                                </div>

                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="control-label">Amount and Tax (10%)</label>


                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-8" style="padding:0;">
                                                    <input type="text" id="txtAmount" name="txtAmount" class="form-control" placeholder="Price" onblur="CalcTaxForAmount()">
                                                </div>
                                                <div class="col-md-4" style="padding:0;">
                                                    <input type="text" id="txtTaxAmount" name="txtTaxAmount" class="form-control" placeholder="Tax" readonly="readonly">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <label class="control-label">Inspect Charges & Tax (10%) </label>


                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-md-8" style="padding:0;">
                                                        <input type="number" id="txtInspectionCharges" name="txtInspectionCharges" class="form-control" placeholder="Inspection Charges" onblur="CalcTaxForInspectionCharges()">
                                                    </div>
                                                    <div class="col-md-4" style="padding:0;">
                                                        <input type="number" id="txtInspectionChargestax" name="txtInspectionChargestax" class="form-control" placeholder="Tax" readonly="readonly">
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>

                                </div>


                                <div class="col-lg-4">

                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <label class="control-label">Total </label>
                                            <input type="text" id="txtTotal" name="txtTotal" class="form-control" placeholder="Total">

                                        </div>
                                    </div>







                                </div>

                            </div>






                        </div>



                    </section>
                </div>

                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Add</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </form>

        </div>
    </div>



</div>
<!-- Modal Update Vanning -->
<div class="modal fade bs-modal-lg" id="UdpateNewVanningModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content" id="ModalContentVanningDetail">



        </div>
    </div>
</div>




