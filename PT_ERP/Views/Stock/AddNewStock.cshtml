@using DAL
@using DAL.Models
@model DAL.oClasses.OStock
@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration

@{
    ViewData["Title"] = "AddNewStock";
    Layout = "~/Views/Shared/_LayoutTablePages.cshtml";
}
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
<style>
    .float {
        position: fixed;
        width: 60px;
        height: 60px;
        bottom: 40px;
        right: 40px;
        background-color: #25d366;
        color: #FFF;
        border-radius: 50px;
        text-align: center;
        font-size: 30px;
        box-shadow: 2px 2px 3px #999;
        z-index: 100;
    }

    .my-float {
        margin-top: 16px;
    }
</style>
@{
    IEnumerable<DAL.Models.AdminRights> ListRights = ViewBag.AdminRolesRights;



    string StockForm_ActionMethod = "";
    int? Stock_ID = ViewBag.Stock_ID;





    //if (Model.purchaseMasterObj != null && Model.purchaseMasterObj.PurchaseMaster_ID > 0)
    if (Stock_ID != 0)
    {
        StockForm_ActionMethod = "UpdateStock";
    }
    else
    {
        StockForm_ActionMethod = "InsertStock";
    }
    bool Add = false;
    bool Edit = false;
    bool Delete = false;
    bool IsAdmin = false;
    bool Print = false;
    bool Excel = false;
    bool View = false;
    var ValAdd = "";
    var ValEdit = "";
    var ValDelete = "";
    var ValIsAdmin = "";
    var ValPrint = "";
    var ValExcel = "";
    var ValView = "";//disabled
    if (ListRights != null)
    {
        foreach (AdminRights items in ListRights.Where(x => x.MenuID == 50))
        {

            Add = items.Add;
            Edit = items.Edit;
            Delete = items.Delete;
            IsAdmin = items.IsAdmin;
            Print = items.Print;
            Excel = items.Excel;
            View = items.View;

        }
        if (Add == true)
        {
            ValAdd = "";
        }

        if (Edit == true)
        {
            ValEdit = "";
        }

        if (Delete == true)
        {
            ValDelete = "";
        }

        if (Print == true)
        {
            ValPrint = "";
        }

        if (Excel == true)
        {
            ValExcel = "";
        }

        if (View == true)
        {
            ValView = "";
        }

    }


    string NAME = "";
    if (Model.attachmentList !=null)
    {
        if (Model.attachmentList.Count() > 0)
        {
            foreach (Pa_Attachments_DAL item in Model.attachmentList)
            {
                NAME = item.File_Name;
            }
        }
    }
    string WhatsApp_Enabled = Configuration.GetSection("AppSettings").GetSection("WhatsApp_Enabled").Value;
    string PDF_Enabled = Configuration.GetSection("AppSettings").GetSection("PDF_Enabled").Value;
    string Stock_Setting = Configuration.GetSection("AppSettings").GetSection("IsStockList_Name").Value;
    string SecondCurrency = Configuration.GetSection("AppSettings").GetSection("SecondCurrency").Value;
    string Drive_LEFT_Dropdown = Configuration.GetSection("AppSettings").GetSection("Drive_LEFT_Dropdown").Value;
    string Drive_RIGHT_Dropdown = Configuration.GetSection("AppSettings").GetSection("Drive_RIGHT_Dropdown").Value;
    string make_country_ID_Dropdown = Configuration.GetSection("AppSettings").GetSection("make_country_ID_Dropdown").Value;
}

<header class="page-header">
    <h2>@ViewBag.SectionHeaderTitle</h2>

    <div class="right-wrapper text-right">
        <ol class="breadcrumbs">
            <li>
                <a href="@ViewBag.Home_Dashboard">
                    <i class="fas fa-home"></i>
                </a>
            </li>
            <li><a asp-area="" asp-controller="Stock" asp-action="AddNewStock"><span>@ViewBag.SectionSub_HeaderTitle</span></a></li>

        </ol>

        <a class="sidebar-right-toggle"></a>
    </div>
</header>






<div class="row" id="SuccessErrorSection">
    @if (TempData["Success"] != null || TempData["Error"] != null)
    {
        <div class="col-lg-12">
            @if (TempData["Success"] != null)
            {
                <div class="alert alert-info">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Success!</strong> @TempData["Success"] <a href="" class="alert-link"></a>
                </div>
            }

            @if (TempData["Error"] != null)
            {
                <div class="alert alert-danger">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Error!</strong> @TempData["Error"] <a href="" class="alert-link"></a>
                </div>
            }
        </div>
    }

</div>


