    <script>

        function GetMakeCodeDetail() {

            debugger;


            var Chassis = document.getElementById("Chassis").value;

            if (Chassis == -1) {

            }
            else {
                $.ajax({

                    type: "Get",
                    url: "/Stock/GetDetailsByCode",
                    data: { //Passing data
                        Code: Chassis
                    },
                    datatype: "json",
                    success: function (response) {
                        debugger
                       
                        $("#Make_ID").val(response.make_ID);
                        //$("#loc").val(response.VehicalCountry);
                        $("#Make_category_ID").val(response.vehicalCategory);
                        $("#Drive").val(response.drive);
                        $("#EngineType").val(response.engine_Type);
                        $("#FUEL_TYPE").val(response.fuel_Type);
                        $("#Door").val(response.door);
                        $("#HS_CODE").val(response.hs_Code);

                        console.log(response);
                        var ID = response.make_ID;
                        var procemessage = "<option value='0'> Please wait...</option>";
                        $(".MakeModelClass").html(procemessage).show();


                        if (!ID) {
                            ID = 0;
                            var emptyAppend = "<option value=''>Select</option>";
                            $(".MakeModelClass").html(emptyAppend).show();

                        } else {
                            $.getJSON("/Stock/GetModelDescListByID", { Make_ID: ID }, function (data) {
                                //first empty the model
                                //debugger
                                $(".MakeModelClass").empty();
                                //Then append select
                                $(".MakeModelClass").append("<option value=''>Select</option>");
                                $.each(data, function (index, row) {
                                    debugger
                                    //Now here appending all options in the sub classification list
                                    //$(".MakeModelClass").append("<option value='" + row.modelDesc_ID + "'>" + row.modelDesciption + "</option>")
                                    var optionText = row.modelDesciption;
                                    $('.MakeModelClass').append($("<option></option>").attr("value", row.modelDesc_ID).text(optionText));
                                });
                                if (response.modelDesc_ID > 0) {
                                    $("#Model_ID").val(response.modelDesc_ID);
                                }
                            });
                        }








                    }
                })
            }

        }

        //---show attachment popup
        function ShowAttachmentMaster(PurchaseMaster_ID) {
            debugger
            var TeamDetailPostBackURL = '/Purchases/GetPurchaseMasterNew_Attachments';

            //--assign value to PurchaseMaster_ID hidden field in the main. Dont remove this fields
            $("#PurchaseMasterMain").val(PurchaseMaster_ID);


            if (!PurchaseMaster_ID) {
                alert("ID is null. Please try again!");
            } else {
                $.ajax({
                    type: "GET",
                    url: TeamDetailPostBackURL,
                    contentType: "application/json; charset=utf-8",
                    data: { "PurchaseMaster_ID": PurchaseMaster_ID },
                    datatype: "json",
                    success: function (data) {


                        if (data.message == undefined) {
                            $('#AttachmentsPurchaseMaster').html(data);


                            //--assign value to PurchaseMaster_ID hidden field. this field is in partial view '_PurchaseListAttachment'
                            $("#PurchaseMaster_ID_Attachment").val(PurchaseMaster_ID);

                            $('#ShowAttachmentMasterModal').modal('show');

                        } else {
                            alert(data.message);
                            $('#ShowAttachmentMasterModal').modal('hide');
                        }

                    },
                    error: function () {
                        alert("Dynamic content load failed.");
                    }
                });
            }



        }


        function GetAllFields(PurchaseDetails_ID) {



            var PurchaseDetails_ID = PurchaseDetails_ID;

            if (!PurchaseDetails_ID) {
                alert("Stock id is null!");
            } else {

                var TeamDetailPostBackURL = '/Purchases/GetPuchaseDetailByID';
                $.ajax({
                    type: "GET",
                    url: TeamDetailPostBackURL,
                    contentType: "application/json; charset=utf-8",
                    data: { "PurchaseDetails_ID": PurchaseDetails_ID },
                    datatype: "json",
                    success: function (data) {
                        debugger

                        if (data.message == undefined) {
                            //alert("Saved Successfully!");
                            $('#Year').html(data);

                            $('#UdpateNewPurchaseModal').modal('show');

                        } else {
                            alert(data.message);
                            //$('#UdpateNewPurchaseModal').modal('hide');
                        }

                    },
                    error: function () {
                        alert("Failured occured. Please try again!");

                    }
                });
            }




        }

        


        //---after successfull submission of attachment
        function mySuccessAttachment(data) {

            debugger
            if (data.message == undefined) {


                $('#AttachmentsPurchaseMaster').html(data);

                //---assign id of purchase master to the hidden field PurchaseMaster_ID_Attachment after partial view refresh
                var PurchaseMaster_ID = document.getElementById("PurchaseMasterMain").value;
