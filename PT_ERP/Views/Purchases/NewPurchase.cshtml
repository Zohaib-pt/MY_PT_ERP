@using DAL
@using DAL.Models
@model DAL.oClasses.OPurchases
@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration


@{ ViewData["Title"] = "NewPurchase";
    Layout = "~/Views/Shared/_LayoutTablePages.cshtml"; }

@{ string Form_formPurchaseMaster_ActionMethod = "";
    int? PurchaseMaster_ID = ViewBag.PurchaseMaster_ID;


    if (Model.purchaseMasterObj != null && Model.purchaseMasterObj.PurchaseMaster_ID > 0)
    {

        Form_formPurchaseMaster_ActionMethod = "UpdatePurchaseMaster";


    }
    else
    {
        Form_formPurchaseMaster_ActionMethod = "InsertPurchaseMaster";


    }
    }



<header class="page-header">
    <h2>@ViewBag.SectionHeaderTitle</h2>

    <div class="right-wrapper text-right">
        <ol class="breadcrumbs">
            <li>
                <a href="@ViewBag.Home_Dashboard">
                    <i class="fas fa-home"></i>
                </a>
            </li>
            <li><a asp-area="" asp-controller="Purchases" asp-action="PurchaseList"><span>@ViewBag.SectionSub_HeaderMainTitle</span></a></li>
            <li><a asp-area="" asp-controller="@ViewBag.BreadCumController" asp-action="@ViewBag.BreadCumAction"><span>@ViewBag.SectionSub_HeaderTitle</span></a></li>

        </ol>

        <a class="sidebar-right-toggle"></a>
    </div>
</header>






<div class="row" id="SuccessErrorSection">
    @if (TempData["Success"] != null || TempData["Error"] != null)
    {
        <div class="col-lg-12">
            @if (TempData["Success"] != null)
            {
                <div class="alert alert-info">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Success!</strong> @TempData["Success"] <a href="" class="alert-link"></a>
                </div>}

            @if (TempData["Error"] != null)
            {
                <div class="alert alert-danger">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <strong>Error!</strong> @TempData["Error"] <a href="" class="alert-link"></a>
                </div>}
        </div>}


</div>

<!-- start: page -->
<section class="card">
    <div class="card-body">
        <div class="invoice">
            <header class="clearfix">
                <div class="row">
                    <div class="col-sm-6 mt-0">
                        <h2 class="h4 mt-0 mb-2 text-dark font-weight-bold">Purchase </h2>

                    </div>

                    <div class="col-sm-6 mt-0 text-right">
                        @if (String.IsNullOrEmpty(Model.purchaseMasterObj.PurchaseStatus))
                        {
                            <h4 class=" mt-0 mb-2 text-dark"><b>Status:</b> <label>Pending</label></h4> }
                        else
                        {
                            <h4 class=" mt-0 mb-2 text-dark"><b>Status:</b> <label>@Model.purchaseMasterObj.PurchaseStatus</label></h4>}


                    </div>



                </div>
            </header>

            <div class="row">
                <div class="col-lg-8">
                    @if (Model.purchaseMasterObj.PurchaseMaster_ID != null)
                    {
                        if (Model.purchaseMasterObj.PurchaseMaster_ID > 0)
                        {

                            <button type="button" id="ButtonSaveMaster" class="btn-sm btn btn btn-success">Update &nbsp;<i class="fas fa-save"></i></button>

                            <button type="button" class="btn btn-sm btn-primary">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                            <button type="button" id="ChangeStatusMaster" class="btn-sm btn btn-warning" data-toggle="modal" data-target="#ChangeStatusPurchaseModal" onclick="ChangeStatusPurchaseMaster('@Model.purchaseMasterObj.PurchaseMaster_ID', '5');">Cancel &nbsp;<i class="fas fa-cross"></i></button>

                            <button type="button" id="DeletePurchaseMaster" class="btn-sm btn btn-danger" data-toggle="modal" data-target="#DeletePurchaseMasterNew" onclick="DeletePurchaseMaster('@Model.purchaseMasterObj.PurchaseMaster_ID');">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>

                            <button type="button" id="print-btn" class="btn-sm btn btn-info">Print &nbsp;<i class="fas fa-print"></i></button>

                            <button type="button" id="btnApproval" class="btn-sm btn btn btn-success" data-toggle="modal" @ViewBag.ApprovalVisibility data-target="#ChangeStatusPurchaseModal" onclick="ChangeStatusPurchaseMaster('@Model.purchaseMasterObj.PurchaseMaster_ID', '2');">Approve &nbsp;<i class="fa fa-check"></i></button>
                            <button type="button" id="btnComplete" class="btn-sm btn btn btn-info" data-toggle="modal" data-target="#ChangeStatusPurchaseModal" onclick="ChangeStatusPurchaseMaster('@Model.purchaseMasterObj.PurchaseMaster_ID','4');">Complete &nbsp;<i class="fa fa-check"></i></button>
                            <button type="button" id="btnAttachment" class="btn-sm btn btn btn-default" @ViewBag.AttachmentVisibility style="background-color: #68889E; color:white; border-color:#2F3E48" onclick="ShowAttachmentMaster('@Model.purchaseMasterObj.PurchaseMaster_ID');">Attachment &nbsp;<i class="fa fa-paperclip"></i></button>
                            <a asp-controller="Purchases" asp-action="PurchaseList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a> }

                    }
                    else
                    {
                        <button type="button" id="ButtonIssue" class="btn-sm btn btn-success">Issue &nbsp;<i class="fas fa-save"></i></button>

                        <button type="button" class="btn-sm btn btn-primary" disabled="disabled">Edit  &nbsp;<i class="fas fa-edit"></i></button>


                        <button type="button" class="btn-sm btn btn-warning" disabled="disabled">Cancel &nbsp;<i class="fas fa-cross"></i></button>
                        <button type="button" class="btn-sm btn btn-danger" disabled="disabled">Delete &nbsp;<i class="fas fa-trash-alt"></i></button>
                        <button type="button" class="btn-sm btn btn-info" disabled="disabled">Print &nbsp;<i class="fas fa-print"></i></button>

                        <button type="button" class="btn-sm btn btn btn-info">Complete &nbsp;<i class="fa fa-check"></i></button>
                        <button type="button" class="btn-sm btn btn btn-success" @ViewBag.ApprovalVisibility disabled="disabled">Approve &nbsp;<i class="fa fa-check"></i></button>


                        <a asp-controller="Purchases" asp-action="PurchaseList" class="btn-sm btn btn-default">List &nbsp;<i class="fas fa-list"></i></a>}


                </div>

            </div>

            <div class="row">

                <div class="col">



                    <form method="post" asp-area="" asp-controller="Purchases" asp-action="@Form_formPurchaseMaster_ActionMethod" id="formPurchaseMaster">
                        @Html.AntiForgeryToken()

                        <section class="card">

                            <div class="card-body">



                                <div class="row form-group">
                                    <div class="col-lg-4">

                                        <div class="row MasterForm">
                                            <label for="Vendor_ID" class="col-sm-3 col-form-label">Vendor</label>
                                            <div class="col-sm-9">
                                                <select class="form-control select2" id="Vendor_ID" name="Vendor_ID" onchange="AddNewVendor(this)" asp-items="@(new SelectList( ViewBag.VendorMaster,"Partner_ID","PartnerName", Model.purchaseMasterObj.Vendor_ID))" required>
                                                    <option value="0">Select Vendor</option>
                                                    <option value="NewVendor">New Vendor A/C</option>
                                                </select>

                                                <input type="hidden" id="PurchaseMaster_ID" name="PurchaseMaster_ID" value="@PurchaseMaster_ID" />
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="To" class="col-sm-3 col-form-label">To</label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" id="Vendor_PruchaseTo" name="Vendor_PruchaseTo" value="@Model.purchaseMasterObj.Vendor_PruchaseTo">
                                            </div>
                                        </div>
                                        <div class="row MasterForm">
                                            <label for="Address" class="col-sm-3 col-form-label">Address</label>
                                            <div class="col-sm-9">
                                                <textarea class="form-control" rows="2" id="Vendor_Address" name="Vendor_Address" placeholder="">@Model.purchaseMasterObj.Vendor_Address</textarea>
                                            </div>
                                        </div>



                                    </div>
                                    <div class="col-lg-4">

                                    </div>

                                    <div class="col-lg-4 ">

                                        <div class="row MasterForm">
                                            <label for="PurchaseRef" class="col-sm-3 col-form-label">Ref No</label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" id="PurchaseRef" name="PurchaseRef" value="@Model.purchaseMasterObj.PurchaseRef" readonly>
                                            </div>
                                        </div>


                                        <div class="row MasterForm">
                                            <label for="PurchaseDate" class="col-sm-3 col-form-label">Date</label>
                                            <div class="col-sm-9">
                                                <div class="input-group">
                                                    <span class="input-group-prepend">
                                                        <span class="input-group-text">
                                                            <i class="fas fa-calendar-alt"></i>
                                                        </span>
                                                    </span>
                                                    @if (Model.purchaseMasterObj.PurchaseDate != null)
                                                    {
                                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="PurchaseDate" name="PurchaseDate" value="@Model.purchaseMasterObj.PurchaseDate" required> }
                                                    else
                                                    {
                                                        <input type="text" data-plugin-datepicker class="form-control" autocomplete="off" id="PurchaseDate" name="PurchaseDate" value="@DateTime.Now.ToString("dd-MMM-yyyy")" required>}

                                                </div>
                                            </div>


                                        </div>



                                        <div class="row MasterForm">
                                            <label for="Discount" class="col-sm-3 col-form-label">VAT Charges</label>
                                            <div class="col-sm-9">
                                                <select class="form-control" id="VAT_Rate" asp-for="@Model.purchaseMasterObj.VAT_Rate" name="VAT_Rate" onchange="CalculateSumTotalOfStock()">
                                                    <option value="0">0%</option>
                                                    <option value="5">5%</option>
                                                </select>
                                            </div>
                                        </div>

                                        <div class="row MasterForm">
                                            <label for="Discount" class="col-sm-3 col-form-label">Discount</label>
                                            <div class="col-sm-9">
                                                <input type="number" class="form-control" id="Discount" name="Discount" step="any" onblur="CalculateSumTotalOfStock()" value="@Model.purchaseMasterObj.Discount">
                                            </div>
                                        </div>
                                    </div>


                                </div>

                            </div>



                        </section>
                    </form>
                </div>

            </div>

            <div class="row m-1">
                <div class="col-lg-3">
                    <h2 class="card-title" style="line-height:normal;"> Details </h2>
                </div>


                <div class="col-lg-9 col-md-9 text-right">
                    <button class="btn-sm btn btn-primary" id="btnNewAdd" data-toggle="modal" data-target="#AddNewPurchaseModal">Add New &nbsp;<i class="fas fa-plus"></i></button>


                </div>
            </div>

            <div id="PaymentDetailSection">

                <table class="table table-responsive-md table-bordered mb-0">
                    <thead>
                        <tr>

                            <th>Edit/Delete</th>
                            <th>Make</th>
                            <th>Make Model</th>
                            <th>Color</th>
                            <th>Qty</th>
                            @*<th>ItemDesc</th>*@
                            <th>Curr</th>
                            <th>Unit Price</th>
                            <th>Amount</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.purchaseDetailList != null)
                        {

                            ViewBag.SubTotal = 0;
                            ViewBag.Vat_ExpTotal = 0;
                            ViewBag.DiscountTotal = 0;
                            ViewBag.Grand_Total = 0;


                            foreach (Pa_PurchaseDetails_DAL item in Model.purchaseDetailList)
                            {

                                ViewBag.SubTotal = item.SubTotal;
                                ViewBag.Vat_ExpTotal = item.Vat_ExpTotal;
                                ViewBag.DiscountTotal = item.DiscountTotal;
                                ViewBag.Grand_Total = item.Grand_Total;


                                <tr>
                                    <td>
                                        <a onclick="UpdatePurchaseDetail('@item.PurchaseDetails_ID');"><img style="width: 20px;" src="~/Img/Icons/edit.png" /></a>
                                        <a onclick="DeletePurchaseDetail('@item.PurchaseDetails_ID');" data-toggle="modal" data-target="#DeletePurchaseDetailModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                                    </td>

                                    <td>@Html.DisplayFor(modelItem => item.Make) <br /> <span class="help-block">  @item.Chassis_No</span> </td>
                                    <td>@Html.DisplayFor(modelItem => item.ModelDesciption)</td>
                                    <td>@Html.DisplayFor(modelItem => item.ColorName)</td>

                                    <td>@Html.DisplayFor(modelItem => item.Quantity)</td>
                                    @*<td></td>*@

                                    <td>@Html.DisplayFor(modelItem => item.Currency_Name) x  @item.Currency_Rate</td>
                                    <td>@Html.DisplayFor(modelItem => item.Unit_Price)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Amount)</td>


                                </tr>
                            }
                        }




                    </tbody>

                </table>
                <div class="col-lg">
                    <a asp-controller="Purchases" asp-action="StockInPO" asp-route-PurchaseRef="@Model.purchaseMasterObj.PurchaseRef">Stock In PO </a>

                </div>

                <div class="invoice-summary">

                    <div class="row">

                        <div class="col-lg-9 col-md-9">

                        </div>

                        <div class="col-lg-3 col-md-3 justify-content-end text-right">
                            <table class="table h6 text-dark">
                                <tbody>


                                    <tr class="b-top-0">
                                        <td colspan="2"> Sub Total:</td>
                                        <td class="text-right" id="SubTotal" style="width:136px">@ViewBag.SubTotal</td>
                                    </tr>
                                    <tr>
                                        <td colspan="2">VAT Charge:</td>
                                        <td class="text-right" id="VAT_ChargeTotal">@ViewBag.Vat_ExpTotal</td>
                                    </tr>

                                    <tr>
                                        <td colspan="2">Discount:</td>
                                        <td class="text-right" id="DiscountTotal">@ViewBag.DiscountTotal</td>
                                    </tr>
                                    <tr class="h6">
                                        <td colspan="2">Total Payable&nbsp;(@ViewBag.DefaultCurrencyDisplay):</td>
                                        <td class="text-right" id="Total_Payable">   @ViewBag.Grand_Total</td>
                                    </tr>



                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>

            </div>


            <hr />
            <div class="row">
                <div class="col-sm-3"><span class="show-grid-block">Created at : <b>@Model.purchaseMasterObj.Created_at </b></span></div>
                <div class="col-sm-3"><span class="show-grid-block">Created by :</span><b>@Model.purchaseMasterObj.Created_by</b></div>
                <div class="col-sm-3"><span class="show-grid-block">Modified at :<b>@Model.purchaseMasterObj.Modified_at </b></span></div>
                <div class="col-sm-3"><span class="show-grid-block">Modified by : <b>@Model.purchaseMasterObj.Modified_by </b></span></div>
            </div>






        </div>


    </div>
</section>




<!-- Modal Insert task -->
<div class="modal fade bs-modal-sm in" id="AddNewPurchaseModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background: #f6f6f6;">


                <h4 class="modal-title">Stock Details-Add</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>

            </div>

            <form method="post" data-ajax="true" data-ajax-complete="myComplete" data-ajax-success="mySuccess" data-ajax-failure="myFailure"
                  asp-area="" asp-controller="Purchases" asp-action="InsertPurchaseDetail" data-ajax-loading="" id="FormInsertPurchaseDetail">

                <div class="modal-body">

                    <section class="card">

                        <div class="card-body">



                            <div class="row form-group">
                                @if (Model.purchaseMasterObj.PurchaseMaster_ID != null)
                                {
                                    <input id="hdPurchaseMaster_ID" name="hdPurchaseMaster_ID" type="hidden" value="@Model.purchaseMasterObj.PurchaseMaster_ID" /> }
                                else

                                {

                                    <input id="hdPurchaseMaster_ID" name="hdPurchaseMaster_ID" type="hidden" value="0" />}
                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "false")
                                {
                                    <div class="col-lg-3 col-md-3">


                                        <div class="form-group">
                                            <label class="col-form-label" for="Make_ID">Make*</label>



                                            <select class="form-control" id="Make_ID" name="Make_ID" asp-items="@(new SelectList(ViewBag.MakeList, "Make_ID", "Make"))" onchange="GetSubModel(this);" required>
                                                <option>Select Make</option>
                                            </select>



                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Model_ID">Model*</label>

                                            <select class="form-control MakeModelClass" id="Model_ID" name="Model_ID" required>
                                                <option value="">Select Model</option>

                                            </select>

                                        </div>
                                    </div>

                                }






                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {



                                    <div class="col-lg-3 col-md-3">

                                        <div class="form-group">
                                            <label class="col-form-label">Chassis No</label>
                                            <input type="text" id="Chassis" name="Chassis" value="" class="form-control" onblur="GetMakeCodeDetail();" placeholder="Enter Chassis">
                                            <label id="ChassisValidationMessage"></label>

                                        </div>
                                    </div>


                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Make_ID">Make*</label>



                                            <select class="form-control" id="Make_ID" name="Make_ID" asp-items="@(new SelectList( ViewBag.MakeList,"Make_ID","Make"))" onchange="GetSubModel(this);" required>
                                                <option>Select Make</option>
                                            </select>



                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Model_ID">Model*</label>

                                            <select class="form-control MakeModelClass" id="Model_ID" name="Model_ID" required>
                                                <option value="">Select Model</option>

                                            </select>

                                        </div>
                                    </div>

                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="ModelYear">Year</label>
                                            <input type="text" id="Year" name="Year" class="form-control" placeholder="Year">

                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="color_exterior_ID">Color(Int)</label>
                                            <select class="form-control" id="color_interior_ID" name="color_interior_ID" asp-items="@(new SelectList( ViewBag.ColorsList,"Color_ID","ColorName"))">
                                                <option value="0">Select Color(Int)</option>
                                            </select>
                                        </div>

                                    </div>}

                                <div class="col-lg-3 col-md-3">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Color">Color</label>


                                        <select class="form-control" id="Color_ID" name="Color_ID" asp-items="@(new SelectList( ViewBag.ColorsList,"Color_ID","ColorName"))">
                                            <option>Select Color</option>
                                        </select>
                                    </div>
                                </div>


                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {

                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="loc_ID">Location</label>

                                            <select class="form-control" name="loc" id="loc"
                                                    asp-items="@(new SelectList(ViewBag.CarLocations,"CarLocation_ID","CarLocation"))">
                                                <option value="0">Select Location</option>
                                            </select>


                                        </div>
                                    </div> }
                                else
                                {
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Quantity">Quantity*</label>


                                            <input type="number" id="Quantity" name="Quantity" class="form-control" onblur="CalculateSumAmount()" placeholder="Quantity" required />

                                        </div>
                                    </div>}

                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {

                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Others">Mileage</label>
                                            <input type="text" id="mileage" name="mileage" class="form-control" placeholder="Mileage" />
                                        </div>
                                    </div>}

                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Make_category_ID">Category</label>



                                            <select class="form-control" id="Make_category_ID" name="Make_category_ID" asp-items="@(new SelectList(ViewBag.VehicleCategory,  "VehCategory_ID", "VehCategoryName"))"
                                                    required>
                                                <option>Select Category</option>
                                            </select>

                                        </div>
                                    </div>}

                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Drive">Drive</label>

                                            <select class="form-control" name="Drive" id="Drive">
                                                <option value="">Select</option>
                                                <option value="Right">Right</option>
                                                <option value="Left">Left</option>
                                            </select>
                                        </div>
                                    </div>}

                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="Used_New">New/Used</label>



                                            <select class="form-control" name="Used_New" id="Used_New">
                                                <option value="N">New</option>
                                                <option value="U">Used</option>
                                            </select>
                                        </div>
                                    </div>}








                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="EngineType">Engine Type</label>
                                            <select class="form-control" name="EngineType" id="EngineType"
                                                   
                                                    asp-items="@(new SelectList(ViewBag.EngineType, "EngineType_ID", "Engine_Power"))">
                                                <option value="">Select</option>
                                            </select>
                                        </div>
                                    </div>}

                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {

                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="FUEL_TYPE">Fuel</label>

                                            <select class="form-control" name="FUEL_TYPE" id="FUEL_TYPE">
                                                <option value="">Select</option>
                                                <option value="PETROL">PETROL</option>
                                                <option value="DEISEL">DEISEL</option>

                                            </select>
                                        </div>
                                    </div>}

                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {

                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="BL_NO">BL No</label>

                                            <input type="text" class="form-control" id="BL_NO" name="BL_NO">
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="BL_NO">Container No</label>

                                            <input type="text" class="form-control" id="Cont_NO" name="Cont_NO">
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3">
                                        <div class="form-group">
                                            <label class="col-form-label" for="BL_NO">Auction Name</label>

                                            <input type="text" class="form-control" id="AuctionName" name="AuctionName">
                                        </div>
                                    </div>
                                }



                                @if (Configuration.GetSection("AppSettings").GetSection("IsDirectStockPurEnabled").Value == "true")
                                {


                                    <div class="col-lg-3 col-md-3">
