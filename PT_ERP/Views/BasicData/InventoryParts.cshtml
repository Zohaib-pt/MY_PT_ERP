<header class="page-header">
    <h2>Basic Data</h2>

    <div class="right-wrapper text-right">
        <ol class="breadcrumbs">
            <li>
                <a href="@ViewBag.Home_Dashboard">
                    <i class="fas fa-home"></i>
                </a>
            </li>
            <li><a asp-area="" asp-controller="BasicData" asp-action="Make"><span>Inventory List</span></a></li>

        </ol>

        <a class="sidebar-right-toggle"></a>
    </div>
</header>


<div class="row">
    @if (TempData["Success"] != null || TempData["Error"] != null)
    {
<div class="col-lg-12">
    @if (TempData["Success"] != null)
    {
<div class="alert alert-info">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
    <strong>Success!</strong> @TempData["Success"] <a href="" class="alert-link"></a>
</div>}

    @if (TempData["Error"] != null)
    {
<div class="alert alert-danger">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
    <strong>Error!</strong> @TempData["Error"] <a href="" class="alert-link"></a>
</div>}
</div>}


</div>

<div class="row">
    <div class="col">
        <section class="card">
            <header class="card-header">

                <div class="row">
                    <div class="col-lg-6">
                        <h2 class="card-title" style="line-height:normal;">Inventory</h2>
                    </div>
                    <div class="col-lg-6" style="text-align:right;">
                        <button class="btn btn-sm btn-primary" data-toggle="modal" data-target="#AddMakeModal">Add Inventory &nbsp;<i class="fas fa-plus"></i></button>
                        <button type="submit" class="mb-1 mt-1 mr-1 btn btn-sm btn-info" id="print-btn">Print &nbsp;<i class="fas fa-print"></i></button>
                        <button type="submit" class="mb-1 mt-1 mr-1 btn btn-sm btn-success" id="export-btn">Export &nbsp;<i class="fas fa-file-excel"></i></button>


                    </div>
                </div>

            </header>
            <div class="card-body">



                <table class="table table-bordered table-striped mb-0" id="datatable-default">
                    <thead>
                        <tr>


                            <th>Edit/Delete</th>
                            <th>Item Code</th>

                            <th>ItemName</th>


                            <th>Compatible Years</th>
                            <th>Engine Specs</th>

                            <th>PO(Qty)</th>
                            <th>PR(Qty)</th>
                            <th>Sale(Qty)</th>
                            <th>Delivered(Qty)</th>
                            <th>Return(Qty)</th>
                            <th>Avaible(Qty)</th>
                            <th>UnitPrice</th>
                            <th>ItemGroup</th>
                            <th>ItemCategory</th>






                        </tr>
                    </thead>
                    <tbody>

                        @if (Model.InventryMasterList != null)
                        {
                            foreach (Pa_Inventry_DAL item in Model.InventryMasterList)
                            {

            <tr>

                <td>
                    <a onclick="UpdateInventory('@item.Item_ID','@item.ItemCode','@item.Group_ID','@item.Category_ID','@item.UOM', '@item.ItemName', '@item.ItemDescription', '@item.IsSerializable', '@item.BarCode', '@item.UnitPrice', '@item.SalePrice', '@item.CostMethod', '@item.Comment', '@item.Traditional', '@item.Transmission', '@item.FUEL_TYPE', '@item.Drive', '@item.StartYear', '@item.EndYear', '@item.OpeningBal')" data-toggle="modal"
                       data-target="#UpdateMakeModal"><img style="width: 20px;" src="~/Img/Icons/edit.png" /></a>
                    <a onclick="DeleteInventory('@item.Item_ID')" data-toggle="modal" data-target="#DeleteMakeModal"><img style="width: 20px;" src="~/Img/Icons/delete.png" /></a>

                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.ItemCode)
                </td>


                <td>
                    @Html.DisplayFor(modelItem => item.ItemName)
                </td>








                <td>
                    @Html.DisplayFor(modelItem => item.Year)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EngineSpecsCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Purchase_Qty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Received_Qty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Sold_Qty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Delivered_Qty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Return_Qty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.InHand_Qty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UnitPrice)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GroupName)

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CategoryName)
                </td>

            </tr>
}
    }

                    </tbody>
                </table>
            </div>
        </section>
    </div>
</div>



<!-- Model Add Inventory -->
<div class="modal fade bs-modal-sm in" id="AddMakeModal" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header" style="background: #f6f6f6;">


                <h4 class="modal-title">Add </h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>

            </div>

            <form method="post" asp-area="" asp-controller="BasicData" asp-action="AddInventryParts">

                <div class="modal-body">

                    <section class="card">

                        <div class="card-body">



                            <div class="row form-group">

                                <div class="col-lg-4">
                                    <div class="form-group">


                                        <label class="col-form-label" for="Project_ID">Item Code</label>
                                        <input type="text" class="form-control" id="ItemCode" name="ItemCode" @Show>
                                    </div>
                                </div>


                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Project_ID">ItemName</label>

                                        <input type="text" class="form-control" id="ItemName" name="ItemName">
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">


                                        <label class="col-form-label" for="Project_ID">Traditional</label>
                                        <input type="text" class="form-control" id="Traditional" name="Traditional">

                                    </div>
                                </div>

                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Project_ID">ItemDescription</label>


                                        <textarea class="form-control" id="ItemDescription" name="ItemDescription" rows="2"></textarea>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Group">Item Group</label>

                                        <select class="form-control" id="Group_ID" name="Group_ID" asp-items="@(new SelectList( ViewBag.ItemGroupList,"Group_ID","Group_Name"))" required >

                                            <option value=""></option>

                                        </select>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Category">Item Category</label>

                                        <select class="form-control" id="Category_ID" name="Category_ID" asp-items="@(new SelectList( ViewBag.ItemCategoryList,"Category_ID","Category_Name"))" required>

                                            <option value=""></option>

                                        </select>
                                    </div>
                                </div>

                                <div class="col-lg-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="FUEL_TYPE">Fuel</label>

                                        <select class="form-control" name="FUEL_TYPE" id="FUEL_TYPE">
                                            <option value=""></option>
                                            <option value="PETROL">PETROL</option>
                                            <option value="DEISEL">DEISEL</option>

                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Transmission">Transmission</label>

                                        <select class="form-control" name="Transmission" id="Transmission">
                                            <option value=""></option>
                                            <option value="A/T">A/T</option>
                                            <option value="M/T">M/T</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Drive">Drive</label>

                                        <select class="form-control" name="Drive" id="Drive">
                                            <option value=""></option>
                                            <option value="2WD">2WD</option>
                                            <option value="4WD">4WD</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-lg-6 hidden">
                                    <div class="form-group">
                                        <label class="col-form-label" for="Project_ID">IsSerializable</label>

                                        <input type="text" class="form-control" id="IsSerializable" name="IsSerializable">


                                    </div>
