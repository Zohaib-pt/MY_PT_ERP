@section Scripts {
    <script>

        $(function () {
            $(".select2").select2({
                theme: "bootstrap",

            });
        });


    </script>
    <script type="text/javascript">
        function DeleteInventory(Item_ID) {

            $("#Item_ID_Delete").val(Item_ID);
            // $("#ItemName_Delete").val(ItemName);
        }




        function MultipleEngineSpecsCode(Item_ID) {


            $.ajax({

                type: "Get",
                url: "/BasicData/Get_MultipleEngineSpecsCode",
                data: { //Passing data
                    Item_ID: Item_ID
                },
                datatype: "json",
                success: function (response) {
                    debugger
                    $('#EngineSpecsCode_ID_Update').val(response.enginespecscode).trigger('change');

                }
            })
        }


        function MultipleMakeLoad(Item_ID) {

            $.ajax({

                type: "Get",
                url: "/BasicData/Get_MultipleMake",
                data: { //Passing data
                    Item_ID: Item_ID


                },


                datatype: "json",
                success: function (response) {

                    debugger
                    //debugger


                    $('#make_ID_Update').val(response.make_ID).trigger('change');





                }
            })
        }
        function UpdateInventory(Item_ID, ItemCode, Group_ID, Category_ID, UOM, ItemName, ItemDescription, IsSerializable, BarCode, UnitPrice, SalePrice, CostMethod, Comment,
            Traditional, Transmission, FUEL_TYPE, Drive, StartYear, EndYear, OpeningBal
        ) {



            $("#Item_ID_Update").val(Item_ID);
            $("#ItemCode_Update").val(ItemCode);
            $("#UOM_Update").val(UOM);
            $("#Group_ID_Update").val(Group_ID);
            $("#Category_ID_Update").val(Category_ID);
            $("#ItemName_Update").val(ItemName);
            $("#ItemDescription_Update").val(ItemDescription);

            $("#IsSerializable_Update").val(IsSerializable);
            $("#BarCode_Update").val(BarCode);

            $("#UnitPrice_Update").val(UnitPrice);
            $("#SalePrice_Update").val(SalePrice);

            $("#CostMethod_Update").val(CostMethod);
            $("#Comment_Update").val(Comment);
            $("#Traditional_Update").val(Traditional);
            $("#FUEL_TYPE_Update").val(FUEL_TYPE);
            $("#Transmission_Update").val(Transmission);
            $("#Drive_Update").val(Drive);
            $("#StartYear_Update").val(StartYear);
            $("#EndYear_Update").val(EndYear);
            $("#OpeningBal_Update").val(OpeningBal);

            MultipleMakeLoad(Item_ID);

            MultipleEngineSpecsCode(Item_ID);

        }



    $(document).ready(function(){
        $("#print-btn").click(function(event){
        event.preventDefault();
            var win = window.open("@Url.Action("InventoryPrintForm", "Report")" + "?Item_ID=" + $("#Item_ID").val()  , "_blank");
        win.focus();
            });
        })
    $(document).ready(function(){
                    $("#export-btn").click(function(event){
                        event.preventDefault();
                        window.location.href = "@Url.Action("InventoryPrintFormExcel", "Report")" + "?Item_ID=" + $("#Item_ID").val();
                    });
                })

    </script>
}