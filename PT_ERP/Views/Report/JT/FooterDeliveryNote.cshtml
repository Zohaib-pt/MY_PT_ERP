@using DAL
@using DAL.Models
@model DAL.oClasses.OSales

@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration
@{
    Layout = null;

    int? CompanyId_ValiDate = ViewBag.ReportCompanyDep_f;

    string CarTakenDate = "  ";
    string CarTakenPerson = "  ";
    string CarTakenContact = "  ";

    string SVRef = "  ";
    string chassis_No = "  ";
    string CarLocation = "  ";
    string CustomerName = "  ";
    string RVNO = "  ";
    string Vehicle = "  ";
    string color = "  ";
    string Notes = "  ";
    string DeliveryRef = "  ";
    string Created_by = "  ";

    string CompanyName = "";

}
@if (CompanyId_ValiDate == 1)
{
    CompanyName = Configuration.GetSection("AppSettings").GetSection("FirstCompanyName").Value;
}
else if(CompanyId_ValiDate ==2) {
    CompanyName = Configuration.GetSection("AppSettings").GetSection("FirstCompanyName").Value;
}
@if (Model.DeliveryReport_Print != null)
{


    foreach (Pa_DeliveryOrder_Master item1 in Model.DeliveryReport_Print)
    {


        CarTakenDate = item1.CarTakenDate;
        CarTakenPerson = item1.CarTakenPerson;
        CarTakenContact = item1.CarTakenContact;

        SVRef = item1.SVRef;
        chassis_No = item1.chassis_No;
        CarLocation = item1.CarLocation;
        CustomerName = item1.CustomerName;
        RVNO = item1.RVNO;
        Vehicle = item1.Vehicle;
        color = item1.color;
        Notes = item1.Notes;
        DeliveryRef = item1.DeliveryRef;
        Created_by = item1.Created_by;



    }
}
