@using DAL
@using DAL.Models
@model DAL.oClasses.OSales
@{ Layout = null;

    var StartDate = @Context.Request.Query["StartDate"];
    var EndDate = @Context.Request.Query["EndDate"];

    }

<!DOCTYPE HTML>
<HTML>
<head>

    <!-- Basic -->
    <meta charset="UTF-8">


    <title>@ViewBag.PageTitle</title>
    <meta name="keywords" content="DearlerSol | Dealers Management System " />
    <meta name="description" content="Vehicle Management System | Dealer Management System | Dealer ERP ">
    <meta name="author" content="DealerSol.com">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Favicon  -->
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico">


    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/vendor/bootstrap/css/bootstrap.css" />


    <style>
        .moneycol {
            text-align: right;
        }

        .controlheight {
            height: 1px;
            border: 1px solid #d3d3d3;
            margin: 0px;
        }

            .controlheight table th, .table td {
                padding: 5px;
            }

        .SumHeading {
            background-color: #d3d3d3;
            text-align: left;
            font-weight: bold;
            font-size: 16px
        }

        .ColHeading {
            background-color: #d3d3d3;
            font-size: 18px;
            text-align: center
        }
    </style>


</head>

<BODY>


    <div class="row">
        <div class="col">
            <section>
                <header>
                    <div class="row form-inline">
                        @* Farzaib *@
                        <div class="col-md-6">
                            <p class="h6 text-right"><b>From</b> @StartDate  <b>Till</b> @EndDate</p>
                            <div class="text-center">
                                
                                <h3>Sales Return List</h3>
                            </div>



                        </div>
                        @*<div class="col-md-12" style="text-align:center">

            <h3 style="color:black;">
                Sale_Return_List
                <br />
            </h3>
            <p><b>From</b> @StartDate  <b>Till</b> @EndDate</p>


        </div>*@


                    </div>

                </header>
                <div>
                    <table class="table table-bordered" @*id="datatable-default"*@>
                        <thead>
                            <tr class="ColHeading">





                                <td>  SaleRef </td>
                                <td>  Date </td>
                                <td>  Customer_Name   </td>
                                <td>  Total_Amt </td>


                            </tr>
                        </thead>
                        <tbody>


                            @if (Model.Get_SalesMaster_Print != null)
                            {


                                foreach (pa_SalesMaster_DAL item in Model.Get_SalesMaster_Print)
                                {


                <tr>


                    <td class="controlheight text-nowrap">@Html.DisplayFor(modelItem => item.SaleRef) </td>
                    <td class="controlheight">@Html.DisplayFor(modelItem => item.Date)</td>
                    <td class="controlheight">@Html.DisplayFor(modelItem => item.Customer_Name) </td>
                    <td class="controlheight ">@Html.DisplayFor(modelItem => item.Total_Amt) </td>







                </tr>}
                            }


                        </tbody>
                    </table>
                </div>
            </section>
        </div>
    </div>
</BODY>
</HTML>