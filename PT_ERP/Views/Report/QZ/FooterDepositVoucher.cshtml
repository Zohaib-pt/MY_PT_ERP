@using DAL
@using DAL.Models
@model DAL.oClasses.oReports

@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration
@{ Layout = null;


                string DV_Ref = "";
                string Date_Taken = "";
                string CUSTOMER_NAME = "";
                string Chassis_No = "";
                string ModelDesciption = "";
                string Make = "";
                string ModelYear = "";
                string Color = "";
                string Deposit = "";
                string AfterTotal = "";
                string AMOUNT_INWORDS = "";
                string refund_Customer = "";
                string refund_Contact = "";
                string REFUND_DATE = "";
                string DEPOSIT_EXP_DATE = "";
                string CAR_SHIP_DATE = "";
                string DEDUCTED_AMOUNT = "";
                string Amount_Return = "";
                string cheque_Date = "";
                string Cheque_no = "";
                string Cheque_Bank_Name = "";


                int? CompanyId_ValiDate = ViewBag.ReportCompanyDep_f; }
@if (Model.DepositVoucher_Print != null)
{

    ViewBag.Total_Amount = 0;
    foreach (Deposits_DAL item1 in Model.DepositVoucher_Print)
    {
        DV_Ref = item1.DV_Ref;
        Date_Taken = item1.Date_Taken;
        CUSTOMER_NAME = item1.CUSTOMER_NAME;
        Chassis_No = item1.Chassis_No;
        ModelDesciption = item1.ModelDesciption;
        Make = item1.Make;
        ModelYear = item1.ModelYear;
        Color = item1.Color;

        AfterTotal = item1.AfterTotal;
        AMOUNT_INWORDS = item1.AMOUNT_INWORDS;
        refund_Customer = item1.refund_Customer;
        refund_Contact = item1.refund_Contact;
        REFUND_DATE = item1.REFUND_DATE;
        DEPOSIT_EXP_DATE = item1.DEPOSIT_EXP_DATE;
        CAR_SHIP_DATE = item1.CAR_SHIP_DATE;
        DEDUCTED_AMOUNT = item1.DEDUCTED_AMOUNT;
        Amount_Return = item1.Amount_Return;

        decimal Total_Amount = Convert.ToDecimal(item1.Total_Amount);
        ViewBag.Total_Amount = Convert.ToDecimal(ViewBag.Total_Amount) + Total_Amount;


    }
}
@if (Model.DepositVoucherCheque_Print != null)
{


    foreach (Deposits_DAL item1 in Model.DepositVoucherCheque_Print)
    {

        cheque_Date = item1.cheque_Date;
        Cheque_no = item1.Cheque_no;
        Cheque_Bank_Name = item1.Cheque_Bank_Name;


    }
}
