<BODY >

    <br />
    <div class="row">
        <div class="col">
            <section>
                <header>

                    @*<div class="row form-group">
                            <div class="col-xs-12">
                                <h6 style="text-align:center">From   - To   </h6>
                            </div>
                        </div>*@
                </header>
                <div>
                    @if (SaleInvoiceType == "ByChassis")
                    {
                        <table class="table table-bordered" @*id="datatable-default"*@>
                            <thead>
                                <tr class="ColHeading">





                                    <th>S.NO</th>


                                    <th>CHASSIS NO</th>
                                    <th>ENGINE NO.</th>
                                    <th>COLOR</th>

                                    <th>YEAR</th>
                                    <th>QTY</th>


                                    <th>UNIT PRICE (@ViewBag.ShortCurrencyName)</th>

                                    



                                </tr>
                            </thead>
                            <tbody>





                                @if (Model.PerformaPrintObj != null)
                                {

                                    foreach (var item in Model.PerformaPrintObj.GroupBy(item => new
                                    {
                                        item.ModelDesciption,
                                        item.Make,
                               

                                }))

                                    {
                                <tr role="row">
                                    <td style="text-align:center" colspan=8><b>@Html.DisplayFor(modelItem => item.Key.Make)&nbsp;@Html.DisplayFor(modelItem => item.Key.ModelDesciption)</b></td>
                               
                                </tr>

                                        int counter = 0;

                                        ViewBag.Total = 0;
                                    
                                        foreach (var item1 in item.Where(m => m.SaleInvoiceType == "ByChassis"))
                                        {
                                            double Total = Convert.ToDouble(string.IsNullOrEmpty(item1.Unit_Price) == true ? "0" : item1.Unit_Price);
                                            ViewBag.Total = Total + ViewBag.Total;

                                         


                                            counter++;
                            <TR>
                                <TD class="controlheight text-center">@counter</TD>

                                <TD class="controlheight">@item1.Chassis_No</TD>
                                <TD class="controlheight">@item1.Engine_No</TD>
                                <TD class="controlheight text-center">@item1.Color</TD>

                                <TD class="controlheight text-center">@item1.ModelYear</TD>
                                <TD class="controlheight text-center">@item1.Quantity</TD>

                                <TD class="controlheight moneycol">@item1.Unit_Price  @item1.Currency_ShortName</TD>
                               
                                
                            </TR>
                                        }
                            <TR>
                                <td><b>SubTotal</b></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                @if (ViewBag.Total != null)
                                {
                                    <TD class="controlheight moneycol"><b>@string.Format("{0:0,0.0,0}", ViewBag.Total) @ViewBag.ShortCurrencyName</TD>
                                }
                                else
                                {
                                    <TD class="controlheight moneycol"><b>@ViewBag.Total @ViewBag.ShortCurrencyName</b></TD>
                                }
                            </TR>
                                    }
                                    <TR>
                                        <TD colspan=2><b>TOTAL</b></TD>
                                        <TD colspan=3></TD>

                                        <TD class="controlheight text-center">@ViewBag.TotalQuantity</TD>
                                        <TD class="controlheight moneycol"><b>@TotalBeforeVAT  @ViewBag.ShortCurrencyName</b></TD>
                                    </TR>
                                    <TR>
                                        <TD colspan=2><b>TAX 5%</b></TD>
                                        <TD colspan=4></TD>
                                        <TD class="controlheight moneycol"><b>@VATExp  @ViewBag.ShortCurrencyName</b></TD>
                                    </TR>
                                    <TR>
                                        <TD colspan=2><b>TOTAL AFTER TAX</b></TD>
                                        <TD colspan=4></TD>
                                        <TD class="controlheight moneycol"><b>@AfterVatTotal @ViewBag.ShortCurrencyName</b></TD>
                                    </TR>
                                    <TR>
                                        <TD colspan=2><b>TOTAL IN WORDS</b></TD>
                                        <TD colspan=5>@Total_Amt_inWords </TD>

                                    </TR>

                                }


                            </tbody>
                        </table>
                    }
                    else if (SaleInvoiceType == "ByService")
                    {
                        <table class="table table-bordered" @*id="datatable-default"*@>
                            <thead>
                                <tr class="ColHeading">





                                    <th>S.NO</th>


                                    <th style="width:500px">Specification</th>

                                    <th>Qty</th>


                                    <th>UNIT PRICE (@ViewBag.ShortCurrencyName)</th>





                                </tr>
                            </thead>
                            <tbody>





                                @if (Model.PerformaPrintObj_Specs != null)
                                {
                                    int counter = 0;
                                    ViewBag.TotalQuantity = 0;

                                    foreach (pa_SalesMaster_DAL item in Model.PerformaPrintObj_Specs.Where(m => m.SaleInvoiceType == "ByService"))
                                    {

                                        double TotalQuantity = Convert.ToDouble(string.IsNullOrEmpty(item.Quantity) == true ? "0" : item.Quantity);
                                        ViewBag.TotalQuantity = Convert.ToDouble(ViewBag.TotalQuantity) + TotalQuantity;

                                        counter++;
                                        <TR>
                                            <TD class="controlheight text-center">@counter</TD>

                                            <TD style="width:500px" class="controlheight text-center">@item.ItemDesc</TD>

                                            <TD class="controlheight text-center">@item.Quantity</TD>

                                            <TD class="controlheight moneycol">@item.Unit_Price  @ViewBag.ShortCurrencyName</TD>

                                        </TR>
                                    }
                                }
                                <TR>
                                    <TD colspan=2>TOTAL</TD>


                                    <TD class="controlheight text-center">@ViewBag.TotalQuantity</TD>
                                    <TD class="controlheight moneycol">@TotalBeforeVAT @ViewBag.ShortCurrencyName</TD>
                                </TR>
                                <TR>
                                    <TD colspan=2>TAX 5%</TD>
                                    <TD colspan=1></TD>
                                    <TD class="controlheight moneycol">@VATExp @ViewBag.ShortCurrencyName</TD>
                                </TR>
                                <TR>
                                    <TD colspan=2>Grand TOTAL</TD>
                                    <TD colspan=1></TD>
                                    <TD class="controlheight moneycol">@AfterVatTotal @ViewBag.ShortCurrencyName</TD>
                                </TR>
                                <TR>
                                    <TD colspan=4>TOTAL IN WORDS: &nbsp;&nbsp;&nbsp;&nbsp; @Total_Amt_inWords </TD>


                                </TR>




                            </tbody>
                        </table>
                    }


                    <br />
                    <br />
                    <div style="page-break-inside:avoid;">
                        <table style="width:50%;">
                            <tr>
                                <th style="border:1px solid #d3d3d3; text-align:center">Bank Details</th>

                            </tr>
                            <tr>
                                <th>Account Holder:</th>
                                <th>@AccountName</th>
                            </tr>

                            <tr>
                                <th>Account Number:</th>
                                <th>@AccountNumber</th>
                            </tr>
                            <tr>
                                <th>Bank Name:</th>
                                <th>@BankName</th>
                            </tr>

                            <tr>
                                <th>IBAN Number:</th>
                                <th>@IBAN</th>
                            </tr>

                            <tr>
                                <th>SWIFT CODE:</th>
                                <th>@SwiftCode</th>
                            </tr>

                            <tr>
                                <th>BANK ADDRESS:</th>
                                <th>@BankAddress</th>
                            </tr>
                        </table>
                    </div>
                </div>

