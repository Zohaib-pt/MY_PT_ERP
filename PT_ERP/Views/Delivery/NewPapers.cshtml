                                            <div class="col-lg-5">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Del. Date</label>
                                                    <input type="date" class="form-control" id="Emp_No" name="Emp_No">
                                                </div>
                                            </div>
                                            <div class="col-lg-2" style="margin-top:30px">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Paper Taken</label>
                                                    <input type="checkbox" name="IsActive" id="IsActive">
                                                </div>
                                            </div>
                                            <div class="col-lg-5">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Name</label>
                                                    <input type="text" class="form-control" id="Emp_No" name="Emp_No">
                                                </div>
                                            </div>
                                            <div class="col-lg-5">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Contact</label>
                                                    <input type="text" class="form-control" id="Emp_No" name="Emp_No">
                                                </div>
                                            </div>



                                        </div>
                                    </div>

                                </div>
                                <div id="ReceivedShip6" class="tab-pane">


                                    <div class="card-body">
                                       
                                        <div class="row form-group">
                                            <div class="col-lg-5">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Rec. Date</label>
                                                    <input type="date" class="form-control" id="Emp_No" name="Emp_No">
                                                </div>
                                            </div>
                                            <div class="col-lg-2" style="margin-top:30px">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Paper Recieved</label>
                                                    <input type="checkbox" name="IsActive" id="IsActive">
                                                </div>
                                            </div>
                                            <div class="col-lg-5">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Received By</label>
                                                    <input type="text" class="form-control" id="Emp_No" name="Emp_No">
                                                </div>
                                            </div>
                                            <div class="col-lg-5">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Ship Date</label>
                                                    <input type="date" class="form-control" id="Emp_No" name="Emp_No">
                                                </div>
                                            </div>



                                        </div>
                                    </div>
                                </div>
                                <div id="Claim6" class="tab-pane">
                                    <div class="card-body">
                                
                                        <div class="row form-group">
                                            <div class="col-lg-5">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Claim Date</label>
                                                    <input type="date" class="form-control" id="Emp_No" name="Emp_No">
                                                </div>
                                            </div>
                                            <div class="col-lg-2" style="margin-top:30px">
                                                <div class="form-group">
                                                    <label class="col-form-label" for="Project_ID">Claim Rec</label>
                                                    <input type="checkbox" name="IsActive" id="IsActive">
                                                </div>
                                            </div>




                                        </div>
                                    </div>

                                </div>





                            </div>
                            </div>
                    </div>
                </div>





















                            


                      


                                



             




                  


        </div>
    </div>
</section>

