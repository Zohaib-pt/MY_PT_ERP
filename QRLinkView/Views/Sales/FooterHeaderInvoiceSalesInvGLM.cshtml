            }

               var CompanyId_ValiDate = @CompanyId_ValiDate;
            var FirstCompanyName= '@Configuration.GetSection("AppSettings").GetSection("FirstCompanyName").Value';
            var SecondCompanyName = '@Configuration.GetSection("AppSettings").GetSection("SecondCompanyName").Value';
            var QRSImageURL = '@Configuration.GetSection("AppSettings").GetSection("QRSImageURL").Value';

            var NAME='@Model.SalesMasterPrintObj.imgname';
            var QRDiv='@Model.SalesMasterPrintObj.QRName';
            //if current page number == last page number
            if (vars['page'] == vars['topage']) {
                if (CompanyId_ValiDate == 1) {


                    var img = document.createElement("IMG");
                    img.src = QRSImageURL + NAME;
                    var img2 = document.createElement("IMG");
                    img2.src = QRSImageURL + QRDiv;

                    document.querySelectorAll('.extra')[0].textContent = FirstCompanyName;
                    document.getElementById('imageDiv').appendChild(img);
                    document.getElementById('QRDiv').appendChild(img2);

                }
                else if (CompanyId_ValiDate == 2) {
                    var img = document.createElement("IMG");
                    img.src = QRSImageURL + NAME;
                    var img2 = document.createElement("IMG");
