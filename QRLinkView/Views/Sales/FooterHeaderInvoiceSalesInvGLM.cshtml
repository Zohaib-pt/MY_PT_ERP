                    document.querySelectorAll('.extra')[0].textContent = SecondCompanyName;
                    document.getElementById('imageDiv').appendChild(img);
                    document.getElementById('QRDiv').appendChild(img2);
                }
            }
            else {

                var a = '<div>&nbsp;</div><div>&nbsp;</div><div>&nbsp;</div>';
                document.querySelectorAll('.extra')[0].innerHTML = a;
                document.getElementById('imageDiv').innerHTML = a;
                document.getElementById('QRDiv').innerHTML = a;

            }
        }
